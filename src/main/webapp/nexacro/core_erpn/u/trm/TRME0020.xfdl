        this.edtALLBDSB_NAME.set_value("");    
	}
	
    this.fn_Search_AcctInfo(arrRtn[0]);	// 계정코드 정보가져오기
	
	
    var sRcti_Ysno = this.fnc_Trim(this.dsTA_ACCTXD.getColumn(0, "RCTI_YSNO"));	//접대자 관리항목
	
