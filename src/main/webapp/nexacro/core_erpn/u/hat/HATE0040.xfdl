	if (this.fnc_DatasetChangeCheck("dsAT_ATTEDY")) {
		if (this.fnc_Message("TMM023") == false) return false;
	}
	
	/*근태월 필수조건 체크*/
	if (this.fnc_Length(this.fnc_Trim(this.calSHRATTE_YMTH.fn_GetMonth())) < 1) {
