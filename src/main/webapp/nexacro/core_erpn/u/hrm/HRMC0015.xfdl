 		sReturnString += " COMM_NAME=" + this.fnc_Quote(this.fnc_Trim(this.dtmSEARCH01.COMM_NAME)); 
 		sReturnString += " SECH_CODE=" + this.fnc_Quote(this.fnc_Trim(this.dtmSEARCH01.SECH_CODE));
 	
