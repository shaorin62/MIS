		sReturnString += " DSPT_ENDD=" + this.fnc_Quote(this.fnc_Trim(this.calSHRDSPT_ENDD.value)); 
		sReturnString += " ADJN_DATE=" + this.fnc_Quote(this.fnc_Trim(this.calSHRADJN_DATE.value)); 
