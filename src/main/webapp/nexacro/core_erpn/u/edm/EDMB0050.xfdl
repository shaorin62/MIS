	}
}
/*---------------------------------+
 | 조회조건 중분류 이벤트|
 +--------------------------------*/
this.cmbSHREDC2_CODE_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)//중분류 선택했을떄 소분류 나타내기
{	if(this.cmbSHREDC2_CODE.text!="전체"){
