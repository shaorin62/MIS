this.cmbSHREDC1_CODE_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo) //대분류 선택했을떄 중분류 나타내기
{	
	if(this.cmbSHREDC1_CODE.text!="전체"){
