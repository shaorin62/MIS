	if(iCNT==0){
		this.fnc_Message("PCM001");
		return  false;
	}	
	//완료
	if(this.rdoSUCC_FLAG.index == 0){
		
	//미완료
	}else if(this.rdoSUCC_FLAG.index == 1){
		this.fnc_Message("PAC001");
	}
	return true;
}


