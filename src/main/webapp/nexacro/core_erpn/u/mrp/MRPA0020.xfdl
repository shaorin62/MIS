this.fn_SearchItemCheck = function () {
	
	if (this.calSHRYEAR_MNTH.fn_GetMonth()==''){
		this.fnc_Message("TMM008","년월");
		return false;
	}else if (this.calSHRYEAR_MNTH2.fn_GetMonth()==''){
