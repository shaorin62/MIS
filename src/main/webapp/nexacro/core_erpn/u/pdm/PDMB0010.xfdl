this.grdPD_JOBNXM_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	var sApprStat = this.dsPD_JOBNXM.getColumn(e.row,"APPR_STAT");
	var sDocuId	  = this.dsPD_JOBNXM.getColumn(e.row,"DOCU_IDXX");
	var sApprSeq  = this.dsPD_JOBNXM.getColumn(e.row,"APPR_SEQN");
		
	if(sApprStat == "N") sAPPRGUBN = "createDoc";
	else if(sApprStat == "T") sAPPRGUBN = "deleteDoc";
	else sAPPRGUBN = "show";
		
	if(e.col == 26){
		if(sApprStat == "N" || sApprStat == "T"){
	
			var sJOBN_NUMB = this.dsPD_JOBNXM.getColumn(e.row,"JOBN_NUMB");
			
			this.dsPD_JOBOXM_copy.clearData();
			
			// 선택한 제작번호만 복사
			this.dsPD_JOBOXM.set_enableevent(false);
			this.dsPD_JOBOXM.filter("JOBN_NUMB=='" + sJOBN_NUMB + "'");
			this.dsPD_JOBOXM_copy.copyData(this.dsPD_JOBOXM, true);
			this.dsPD_JOBOXM.filter("");
			this.dsPD_JOBOXM.set_enableevent(true);
			
			this.fn_apprProcess(sAPPRGUBN);
		
		} else{
			/*결재상태가 진행중이거나 완료일 때는 결재문서 pdf 를 보여준다.*/
