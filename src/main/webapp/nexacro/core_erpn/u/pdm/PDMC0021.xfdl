	if (!this.fn_SaveItemCheck()) return;
	
	if(this.dsPD_ESTIMH.getColumn(this.dsPD_ESTIMH.rowposition, "CONF_FLAG") == "견적확정"){
		this.fnc_Message("TMM300","확정된 제작건에 데이터를 저장할 수 없습니다.");
