this.fn_Delete = function (obj) {

	//선택된 데이터 삭제
	if (!this.fn_DeleteCheckDtl()) return;
	
	for(var i = this.dsPD_ESTIMD.rowcount; i >= 0; i--){
		if(this.dsPD_ESTIMD.getColumn(i, "CHK") == 1){
			this.dsPD_ESTIMD.deleteRow(i);
		}
	}
}

