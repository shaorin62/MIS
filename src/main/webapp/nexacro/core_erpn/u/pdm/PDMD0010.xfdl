		if (this.dsPD_SUJUMD.getRowCount() < 1) {
			this.fnc_Message("TMM007");
			return false;
		}
		11111
		var cnt = 0;
		for(var i = 0; i < this.dsPD_SUJUMD.rowcount; i++){
			if(this.dsPD_SUJUMD.getColumn(i, "CHK") == 1){
				cnt += 1;
			}
		}
		
		if(cnt==0){
			alert("선택된 데이터가 없습니다."); 
			return  false;
