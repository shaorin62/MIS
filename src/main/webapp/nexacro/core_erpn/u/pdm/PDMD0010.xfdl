	if (!this.fn_SaveItemCheck()) return;
	
	
	//제작정산에서 상단은 수정할 수 없기 때문에 이후 로직 없음 추후 사용자 요청시 진행
	if(this.fnc_DatasetChangeCheck("dsPD_SUJUMH")){
