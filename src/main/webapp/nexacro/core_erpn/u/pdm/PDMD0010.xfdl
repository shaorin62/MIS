		}else{
			var msg =  cnt + " 건";
		}
		
		
		var sQuestionText = "(" + this.fnc_Trim(this.dsPD_SUJUMD.getColumn(this.dsPD_SUJUMD.rowposition, "SUBX_NAME")) + ") 견적항목 "  + msg ;
