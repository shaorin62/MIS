		sReturnString += " FORI_AMNT1=" + this.fnc_Quote(this.fnc_Trim(this.mskSHRFORI_AMNT1.value));
		sReturnString += " FORI_AMNT2=" + this.fnc_Quote(this.fnc_Trim(this.mskSHRFORI_AMNT2.value));
		
