	if (this.fn_DeleteCheck("dsTA_SLGBXM")) this.dsTA_SLGBXM.deleteRow(this.dsTA_SLGBXM.rowposition);
	this.grdTA_SLGBXM.setFocus();

}

/*------------------------+
