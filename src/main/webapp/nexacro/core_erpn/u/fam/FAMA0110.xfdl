this.dsTA_JUNLXM_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo) {
	if ( e.columnid == "ACCT_NAME" ) this.fn_HelpDialoge(this.grdTA_JUNLXM, "YES", e.oldvalue, e.columnid);
	
