		this.fnc_HelpDialogeAction(this, obj, arrParam);

    } else if ((obj.name == "imgHelpSHRCLSF_CODE") || (obj.name == "edtSHRCLSF_NAME")) {

        arrParam[0] = "FSM0001";                                //FSM001
        arrParam[1] = "D%";                                      
