{
	//this.mskDPCT_RATE.set_value(this.fn_DpctMeth);
	
			this.edtDPCT_ACT2_NAME.set_enable(true);			
			this.edtDPCT_ACT4_NAME.set_enable(true);
		    this.edtDPCT_ACT7_NAME.set_enable(true);
}



