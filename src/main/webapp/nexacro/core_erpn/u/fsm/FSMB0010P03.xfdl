 ************************/
include "script::lib_script_common.xjs"

this.sBUTTONLIST 	= "";
this.sACCT_YEAR 	= "";
this.sPACKAGENAME 	= "FSMB0010P03";
this.sPACKAGENAMERate = "FSMB0010";

/*********************
