}

/*------------------------+
 |  03.조회 버튼 클릭 時  |
 +------------------------*/
this.fn_Search = function (obj) {
	// 필수조회조건 체크
	if (!this.fn_SearchItemCheck()) return;

	this.fnc_DatasetClear("dsYEA_COMP_MED1,dsYEA_COMP_MED2,dsYEA_COMP_MED,dsYEA_COMP_MED3");

	var sMethodName = "SEARCH00";
	var sInDataSet = "";
	var sOutDataSet = "dsYEA_COMP_MED1=dsYEA_COMP_MED1 dsYEA_COMP_MED2=dsYEA_COMP_MED2 dsYEA_COMP_MED=dsYEA_COMP_MED dsYEA_COMP_MED3=dsYEA_COMP_MED3";
	var sArgument = this.fn_CreateArgument(sMethodName);

	this.fnc_TransactionCall(this, this.sPACKAGENAME, sMethodName, sInDataSet, sOutDataSet, sArgument, "fn_CallBack");
	this.grdYEA_COMP_MED1.setFocus(); //재 클릭 방지를 위해 Focus 이동
}

