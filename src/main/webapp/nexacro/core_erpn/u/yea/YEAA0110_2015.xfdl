	if (this.fnc_Length(this.fnc_Trim(this.parent.medSHRYEA_STD_YY.value)) != 4) {
		return this.fnc_SearchCheckPostAction("TMM072", "정산년도", this.parent.medSHRYEA_STD_YY);
