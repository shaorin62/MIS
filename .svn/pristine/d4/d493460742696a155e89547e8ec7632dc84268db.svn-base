<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
		"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="EDMA0050">
<!--
/*******************************************************************************
     작  성  자 : 김혜림
     작  성  일 : 2016.09.05
     내      용 : 교육대분류 데이터 조회
********************************************************************************/
-->
	<select id="EDMA0050.SEARCH00" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[
		SELECT  /* EDMA0050.SEARCH00.교육 대분류조회 */
			EDCY_CODE, /*분류코드*/
			EDC1_CODE, /*대분류코드*/
			EDC2_CODE, /*중분류코드*/
			EDC3_CODE, /*소분류코드*/
			EDCY_NAME, /*코드명*/
			SORT_ORDR,   /*정렬순서*/
			USEX_YSNO, /*사용여부*/
			REMK_100X  /*비고*/
	    FROM ED_CFCTXM
		WHERE 1=1
		  AND EDC1_CODE !='00'
		  AND EDC2_CODE ='00'
		  AND EDC3_CODE ='00'
   ]]>
	<isNotEmpty prepend="AND" property="EDC1_CODE">
		 EDC1_CODE= #EDC1_CODE#
	</isNotEmpty> 
	<![CDATA[ 
		ORDER BY SORT_ORDR, EDCY_CODE
	]]>
	</select>
	
<!--
/*******************************************************************************
     작  성  자 : 김혜림
     작  성  일 : 2016.09.05
     내      용 : 교육중분류 데이터 조회
********************************************************************************/
-->
	<select id="EDMA0050.SEARCH01" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[
		SELECT  /* EDMA0050.SEARCH01.교육 중분류조회 */
			EDCY_CODE, /*분류코드*/
			EDC1_CODE, /*대분류코드*/
			EDC2_CODE, /*중분류코드*/
			EDC3_CODE, /*소분류코드*/
			EDCY_NAME, /*코드명*/
			SORT_ORDR,   /*정렬순서*/
			USEX_YSNO, /*사용여부*/
			REMK_100X  /*비고*/
	    FROM ED_CFCTXM
		WHERE 1=1
		AND EDC1_CODE !='00'
		AND EDC2_CODE !='00'
		AND EDC3_CODE ='00'
   ]]>
	<isNotEmpty prepend="AND" property="EDC1_CODE">
		 EDC1_CODE= #EDC1_CODE#
	</isNotEmpty> 
	<![CDATA[ 
		ORDER BY SORT_ORDR, EDCY_CODE
	]]>
	</select>
	
<!--
/*******************************************************************************
     작  성  자 : 김혜림
     작  성  일 : 2016.09.05
     내      용 : 교육소분류 데이터 조회
********************************************************************************/
-->
	<select id="EDMA0050.SEARCH02" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[
		SELECT  /* EDMA0050.SEARCH02.교육 소분류조회 */
			EDCY_CODE, /*분류코드*/
			EDC1_CODE, /*대분류코드*/
			EDC2_CODE, /*중분류코드*/
			EDC3_CODE, /*소분류코드*/
			EDCY_NAME, /*코드명*/
			SORT_ORDR,   /*정렬순서*/
			USEX_YSNO, /*사용여부*/
			REMK_100X  /*비고*/
	    FROM ED_CFCTXM
		WHERE 1=1
		AND EDC1_CODE !='00'
		AND EDC2_CODE !='00'
		AND EDC3_CODE !='00'
   ]]>
	<isNotEmpty prepend="AND" property="EDC1_CODE">
		 EDC1_CODE= #EDC1_CODE#
	</isNotEmpty> 
	<isNotEmpty prepend="AND" property="EDC2_CODE">
	     EDC2_CODE= #EDC2_CODE#
	</isNotEmpty> 
	<![CDATA[ 
		ORDER BY SORT_ORDR, EDCY_CODE
	]]>
	</select>	
	
<!--
/*******************************************************************************
     작  성  자 : 김혜림
     작  성  일 : 2016.09.05
     내      용 : 교육대분류 중복체크
********************************************************************************/
-->
	<select id="EDMA0050.SEARCH03" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[
		SELECT  DISTINCT(EDC1_CODE) /*교육 대분류코드 조회*/
		FROM ED_CFCTXM 
	]]>
	</select>

<!--
/*******************************************************************************
     작  성  자 : 김혜림
     작  성  일 : 2016.09.05
     내      용  : 분류 저장 
********************************************************************************/
-->
	<insert id="EDMA0050.SAVE00" parameterClass="hashmap">
	<![CDATA[
		INSERT INTO ED_CFCTXM ( /* EDMA0050.SAVE00.교늉분류저장*/
               EDCY_CODE        /*분류코드*/
              ,EDC1_CODE        /*대분류코드*/
              ,EDC2_CODE        /*중분류코드*/
              ,EDC3_CODE        /*소분류코드*/
              ,EDCY_NAME        /*분류명*/
              ,SORT_ORDR          /*정렬순서*/
              ,USEX_YSNO        /*사용여부*/
              ,REMK_100X        /*비고100*/
              ,INST_USID        /*입력작업자*/
              ,INST_DATE        /*입력일시*/
              ,UPDT_USID        /*수정작업자*/
              ,UPDT_DATE        /*수정일시*/
              )
       VALUES (
               #EDCY_CODE#       /*분류코드*/
              ,#EDC1_CODE#       /*대분류코드*/
              ,#EDC2_CODE#       /*중분류코드*/
              ,#EDC3_CODE#       /*소분류코드*/
              ,#EDCY_NAME#       /*분류명*/
              ,#SORT_ORDR#         /*정렬순서*/
              ,#USEX_YSNO#       /*사용여부*/
              ,#REMK_100X#       /*비고100*/
              ,#UPDT_USID#       /*입력작업자*/
              ,SYSDATE           /*입력일시*/
              ,#UPDT_USID#       /*수정작업자*/
              ,SYSDATE           /*수정일시*/
              )

	]]>
	</insert>
	
<!--
/*******************************************************************************
     작  성  자 : 김혜림
     작  성  일 : 2016.09.05
     내      용  : 대분류 수정 
********************************************************************************/
-->		
	<update id="EDMA0050.UPDATE00" parameterClass="hashmap" >
	<![CDATA[
		 UPDATE ED_CFCTXM  /* EDMA0050.UPDATE00.교육분류수정 */
            SET EDCY_NAME = #EDCY_NAME#  /*분류명*/
               ,SORT_ORDR = #SORT_ORDR#    /*정렬순서*/
               ,USEX_YSNO = #USEX_YSNO#  /*사용여부*/
               ,REMK_100X = #REMK_100X#  /*비고100*/
               ,UPDT_USID = #UPDT_USID#  /*수정작업자*/
               ,UPDT_DATE = SYSDATE      /*수정일시*/
         WHERE EDCY_CODE  = #EDCY_CODE#  /*분류코드*/
	]]>
	</update>
	
	
<!--
/*******************************************************************************
     작  성  자 : 김혜림
     작  성  일 : 2016.09.05
     내      용 : 교육대분류삭제
********************************************************************************/
-->
	<delete id="EDMA0050.DELETE00" parameterClass="hashmap" >
	<![CDATA[
	     DELETE FROM ED_CFCTXM  /* EDMA0050.DELETE00.교육대분류삭제 */
         WHERE EDC1_CODE = #EDC1_CODE#   /*분류코드*/
	]]>
	</delete>

<!--
/*******************************************************************************
     작  성  자 : 김혜림
     작  성  일 : 2016.09.05
     내      용 : 교육중분류삭제
********************************************************************************/
-->
	<delete id="EDMA0050.DELETE02" parameterClass="hashmap" >
	<![CDATA[
	     DELETE FROM ED_CFCTXM  /* EDMA0050.DELETE01.교육중분류삭제 */
         WHERE EDC1_CODE = #EDC1_CODE#
           AND EDC2_CODE = #EDC2_CODE#   /*분류코드*/
	]]>
	</delete>

<!--
/*******************************************************************************
     작  성  자 : 김혜림
     작  성  일 : 2016.09.05
     내      용 : 교육소분류삭제
********************************************************************************/
-->
	<delete id="EDMA0050.DELETE03" parameterClass="hashmap" >
	<![CDATA[
	     DELETE FROM ED_CFCTXM  /* EDMA0050.DELETE02.교육소분류삭제 */
         WHERE EDC1_CODE = #EDC1_CODE#
           AND EDC2_CODE = #EDC2_CODE#
           AND EDC3_CODE = #EDC3_CODE#   /*분류코드*/
	]]>
	</delete>

</sqlMap>
