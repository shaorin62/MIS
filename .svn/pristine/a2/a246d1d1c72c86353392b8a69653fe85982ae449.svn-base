}

/*-----------------------------------+
|  변경요청 내용변경 時  |
	@parameter :
	@return :
+------------------------------------*/
this.dsHR_FRLARQ_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo){

    if (e.columnid == "OACT_NAME" || e.columnid == "DFLG_NAME") {
        this.fn_HelpDialoge(this.grdHR_FRLARQ, "YES" ,e.oldvalue, e.columnid);
    }

	var iRow = e.row;

	if (this.dsHR_FRLARQ.getColumn(iRow, "CHDV_CODE") == "D") return; // 변경구분코드

	var vSRAL_NUMB = this.dsHR_FRLARQ.getColumn(iRow, "SRAL_NUMB");
	var iFindRow = this.dsHR_FRLANG.findRow("SRAL_NUMB", vSRAL_NUMB);

	if (iFindRow < -1) {
		this.dsHR_FRLARQ.setColumn(row, "CHDV_CODE", "I");

	} else {

		for(var i = 0; i < this.dsHR_FRLANG.getColCount(); i++) {
			//원본데이터를 기준으로 비교하면 요청데이터의 변경구분 컬럼은 비교 안함.
			if (this.fnc_Quote(this.dsHR_FRLANG.getColumn(iFindRow, this.dsHR_FRLANG.getColID(i))) != this.fnc_Quote(this.dsHR_FRLARQ.getColumn(iFindRow, this.dsHR_FRLANG.getColID(i)))){
				this.dsHR_FRLARQ.setColumn(iRow, "CHDV_CODE", "U");
				return;
			}

		}
	}
	
}

this.dsHR_MSTCRQ_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo){
	if (this.fnc_DatasetChangeCheck("dsHR_FRLARQ")) {
		if (this.fnc_Message("TMM023") == false)  {
			return false;
		}else{
			return true;
		}
	}
}

