	<select id="PDMB0010.SEARCH03" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[ 
		SELECT  NVL(MAX(APPR_SEQN) + 1, TO_CHAR(SYSDATE,'YYYYMMDD')||LPAD('1',5,0)) APPR_SEQN
		  FROM  TM_APPRXH
         WHERE  APPR_SEQN LIKE TO_CHAR(SYSDATE,'YYYYMMDD')||'%'
	]]>
