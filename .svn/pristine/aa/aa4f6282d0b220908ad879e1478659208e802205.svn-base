	<!-- 검색 조건 -->
	<sql id="search">
	    <isNotEmpty prepend="AND" property="YEAR_MONT1" >
			 SUBSTR(B.DEND_DATE,0,6) BETWEEN #YEAR_MONT1# AND '999999'
		</isNotEmpty>
		
		<isNotEmpty prepend="AND" property="YEAR_MONT2" >
			SUBSTR(B.DEND_DATE,0,6) BETWEEN '000000' AND #YEAR_MONT2#
		</isNotEmpty>
		
		<isNotEmpty prepend="AND" property="CUST_CODE" >
			A.OUTS_CODE = #CUST_CODE#
		</isNotEmpty>
		
		<isNotEmpty prepend="AND" property="CUST_NAME" >
			E.CUST_NAME LIKE '%' || #CUST_NAME# || '%'
		</isNotEmpty>
		
		<isNotEmpty prepend="AND" property="CUST_CODE00" >
			B.CUST_CODE = #CUST_CODE00#
		</isNotEmpty>
		
		<isNotEmpty prepend="AND" property="JOBN_NAME" >
			B.JOBN_NAME LIKE '%' || #JOBN_NAME# || '%'
		</isNotEmpty>
		
		
		
	</sql>
	
	<!-- 
	/***********************************************************************************
     작  성  자 : 오세훈
     작  성  일 : 2016.11.21
     내      용 : 제작 결산관리 실적집계표(외주처별 개인별 상세)
     파라미터   :
   /***********************************************************************************
     -->
	<select id="PRPA0100.SEARCH00" parameterClass="hashmap" resultClass="hashmap">
	        <![CDATA[ 
	        
	        
	        SELECT
			CUST_NAME
			, CLAS_NAME
			, JOBN_NUMB
			, JOBN_NAME
			, OUTS_CODE
			, OUTS_NAME
			, SUBX_NAME AS ITEM_NAME
			, NVL(SUJU_AMTX,0) AS SUJU_AMTX
			, NVL(COMM_SION,0) AS COMM_SION
			, NVL(DEND_AMTX,0) AS DEND_AMTX
			, ROW_NUM
			FROM (
			    SELECT
			    '1' GBN
			    , B.CUST_CODE 
			    , C.CUST_NAME
			    , D.CLAS_CODE
			    , D.CLAS_NAME
			    , A.JOBN_NUMB
			    , B.JOBN_NAME
			    , F.SUBX_NAME
			    , A.OUTS_CODE
			    , E.CUST_NAME AS OUTS_NAME 
			    , SUM(A.SUJU_AMTX) AS SUJU_AMTX
			    , SUM(A.DEND_AMTX) - SUM(A.SUJU_AMTX) AS COMM_SION
			    , SUM(A.DEND_AMTX) AS DEND_AMTX
			    , G.ROW_NUM
			    FROM PD_SUJUMD A
			    LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			    LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			    LEFT JOIN (
			        SELECT CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			        WHERE 1=1
			        AND MEMO_FILD = 'TYPE'
			        GROUP BY CLAS_CODE, CLAS_NAME
			    )D ON B.MEDA_CODE = D.CLAS_CODE
			    LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			    LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			    LEFT JOIN (
			        SELECT 
			         A.OUTS_CODE
			        , SUM(A.DEND_AMTX) AS DEND_AMTX
			        , ROW_NUMBER() OVER (ORDER BY  SUM(A.DEND_AMTX) DESC) AS ROW_NUM
			        FROM PD_SUJUMD A
			        LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			        LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			        LEFT JOIN (
			            SELECT CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			            WHERE 1=1
			            AND MEMO_FILD = 'TYPE'
			            GROUP BY CLAS_CODE, CLAS_NAME
			        )D ON B.MEDA_CODE = D.CLAS_CODE
			        LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			        LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			        WHERE 1=1
			            ]]>
				     <include refid="search" />	
					<![CDATA[
			        GROUP BY  A.OUTS_CODE
			    )G ON A.OUTS_CODE = G.OUTS_CODE
			    WHERE 1=1
			    ]]>
				     <include refid="search" />	
					<![CDATA[
			    GROUP BY B.CUST_CODE, C.CUST_NAME, D.CLAS_NAME, D.CLAS_CODE, A.JOBN_NUMB
			    , B.JOBN_NAME, A.OUTS_CODE, E.CUST_NAME,F.SUBX_NAME, G.ROW_NUM
			    UNION ALL
			    SELECT
			    '2' GBN
			    , B.CUST_CODE
			    , C.CUST_NAME
			    , D.CLAS_CODE
			    , D.CLAS_NAME
			    , A.JOBN_NUMB
			    , '계' AS JOBN_NAME
			    , '' AS SUBX_NAME
			    , NULL AS OUTS_CODE
			    , NULL AS OUTS_NAME 
			    , SUM(A.SUJU_AMTX) AS SUJU_AMTX
			    , SUM(A.DEND_AMTX) - SUM(A.SUJU_AMTX) AS COMM_SION
			    , SUM(A.DEND_AMTX) AS DEND_AMTX
			    , NULL  AS ROW_NUM
			    FROM PD_SUJUMD A
			    LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			    LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			    LEFT JOIN (
			        SELECt CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			        WHERE 1=1
			        AND MEMO_FILD = 'TYPE'
			        GROUP BY CLAS_CODE, CLAS_NAME
			    )D ON B.MEDA_CODE = D.CLAS_CODE
			    LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			    LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			        LEFT JOIN (
			        SELECT 
			         A.OUTS_CODE
			        , SUM(A.DEND_AMTX) AS DEND_AMTX
			        , ROW_NUMBER() OVER (ORDER BY  SUM(A.DEND_AMTX) DESC) AS ROW_NUM
			        FROM PD_SUJUMD A
			        LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			        LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			        LEFT JOIN (
			            SELECT CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			            WHERE 1=1
			            AND MEMO_FILD = 'TYPE'
			            GROUP BY CLAS_CODE, CLAS_NAME
			        )D ON B.MEDA_CODE = D.CLAS_CODE
			        LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			        LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			        WHERE 1=1
			            ]]>
				     <include refid="search" />	
					<![CDATA[
			        GROUP BY  A.OUTS_CODE
			    )G ON A.OUTS_CODE = G.OUTS_CODE
			    WHERE 1=1
			    ]]>
				     <include refid="search" />	
					<![CDATA[
			    GROUP BY B.CUST_CODE, C.CUST_NAME, D.CLAS_NAME, D.CLAS_CODE, A.JOBN_NUMB
			    , B.JOBN_NAME
			    UNION ALL
			    SELECT
			    '3' GBN
			    , B.CUST_CODE
			    , C.CUST_NAME
			    , D.CLAS_CODE
			    , D.CLAS_NAME
			    , NULL AS JOBN_NUMB
			    , TO_CHAR(D.CLAS_NAME || ' 소계') AS JOBN_NAME
			    , NULL AS SUBX_NAME
			    , NULL AS OUTS_CODE
			    , NULL AS OUTS_NAME 
			    , SUM(A.SUJU_AMTX) AS SUJU_AMTX
			    , SUM(A.DEND_AMTX) - SUM(A.SUJU_AMTX) AS COMM_SION
			    , SUM(A.DEND_AMTX) AS DEND_AMTX
			    , NULL  AS ROW_NUM
			    FROM PD_SUJUMD A
			    LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			    LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			    LEFT JOIN (
			        SELECt CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			        WHERE 1=1
			        AND MEMO_FILD = 'TYPE'
			        GROUP BY CLAS_CODE, CLAS_NAME
			    )D ON B.MEDA_CODE = D.CLAS_CODE
			    LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			    LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			        LEFT JOIN (
			        SELECT 
			         A.OUTS_CODE
			        , SUM(A.DEND_AMTX) AS DEND_AMTX
			        , ROW_NUMBER() OVER (ORDER BY  SUM(A.DEND_AMTX) DESC) AS ROW_NUM
			        FROM PD_SUJUMD A
			        LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			        LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			        LEFT JOIN (
			            SELECT CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			            WHERE 1=1
			            AND MEMO_FILD = 'TYPE'
			            GROUP BY CLAS_CODE, CLAS_NAME
			        )D ON B.MEDA_CODE = D.CLAS_CODE
			        LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			        LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			        WHERE 1=1
			            ]]>
				     <include refid="search" />	
					<![CDATA[
			        GROUP BY  A.OUTS_CODE
			    )G ON A.OUTS_CODE = G.OUTS_CODE
			    WHERE 1=1
			    ]]>
				     <include refid="search" />	
					<![CDATA[
			    GROUP BY B.CUST_CODE, C.CUST_NAME, D.CLAS_NAME, D.CLAS_CODE
			    UNION ALL
			    SELECT
			    '4' GBN
			    , B.CUST_CODE
			    , C.CUST_NAME
			    , NULL AS CLAS_CODE
			    , NULL AS CLAS_NAME
			    , NULL AS JOBN_NUMB
			    , TO_CHAR(C.CUST_NAME || ' 소계') AS JOBN_NAME
			    , NULL AS SUBX_NAME
			    , NULL AS OUTS_CODE
			    , NULL AS OUTS_NAME 
			    , SUM(A.SUJU_AMTX) AS SUJU_AMTX
			    , SUM(A.DEND_AMTX) - SUM(A.SUJU_AMTX) AS COMM_SION
			    , SUM(A.DEND_AMTX) AS DEND_AMTX
			    , NULL  AS ROW_NUM    
			    FROM PD_SUJUMD A
			    LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			    LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			    LEFT JOIN (
			        SELECt CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			        WHERE 1=1
			        AND MEMO_FILD = 'TYPE'
			        GROUP BY CLAS_CODE, CLAS_NAME
			    )D ON B.MEDA_CODE = D.CLAS_CODE
			    LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			    LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			        LEFT JOIN (
			        SELECT 
			         A.OUTS_CODE
			        , SUM(A.DEND_AMTX) AS DEND_AMTX
			        , ROW_NUMBER() OVER (ORDER BY  SUM(A.DEND_AMTX) DESC) AS ROW_NUM
			        FROM PD_SUJUMD A
			        LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			        LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			        LEFT JOIN (
			            SELECT CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			            WHERE 1=1
			            AND MEMO_FILD = 'TYPE'
			            GROUP BY CLAS_CODE, CLAS_NAME
			        )D ON B.MEDA_CODE = D.CLAS_CODE
			        LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			        LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			        WHERE 1=1
			            ]]>
				     <include refid="search" />	
					<![CDATA[
			        GROUP BY  A.OUTS_CODE
			    )G ON A.OUTS_CODE = G.OUTS_CODE
			    WHERE 1=1
			    ]]>
				     <include refid="search" />	
					<![CDATA[
			    GROUP BY B.CUST_CODE, C.CUST_NAME
			     UNION ALL
			    SELECT
			    '5' GBN
			    , TO_CHAR('ZZZZZZ') AS CUST_CODE
			    , NULL AS CUST_NAME
			    , NULL AS CLAS_CODE
			    , NULL AS CLAS_NAME
			    , NULL AS JOBN_NUMB
			    , TO_CHAR('합계') AS JOBN_NAME
			    , NULL AS SUBX_NAME
			    , NULL AS OUTS_CODE
			    , NULL AS OUTS_NAME 
			    , SUM(A.SUJU_AMTX) AS SUJU_AMTX
			    , SUM(A.DEND_AMTX) - SUM(A.SUJU_AMTX) AS COMM_SION
			    , SUM(A.DEND_AMTX) AS DEND_AMTX
			    , NULL  AS ROW_NUM
			    FROM PD_SUJUMD A
			    LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			    LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			    LEFT JOIN (
			        SELECt CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			        WHERE 1=1
			        AND MEMO_FILD = 'TYPE'
			        GROUP BY CLAS_CODE, CLAS_NAME
			    )D ON B.MEDA_CODE = D.CLAS_CODE
			    LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			    LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			        LEFT JOIN (
			        SELECT 
			         A.OUTS_CODE
			        , SUM(A.DEND_AMTX) AS DEND_AMTX
			        , ROW_NUMBER() OVER (ORDER BY  SUM(A.DEND_AMTX) DESC) AS ROW_NUM
			        FROM PD_SUJUMD A
			        LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
			        LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
			        LEFT JOIN (
			            SELECT CLAS_CODE, CLAS_NAME FROM PD_CODEXM
			            WHERE 1=1
			            AND MEMO_FILD = 'TYPE'
			            GROUP BY CLAS_CODE, CLAS_NAME
			        )D ON B.MEDA_CODE = D.CLAS_CODE
			        LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
			        LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
			        WHERE 1=1
			            ]]>
				     <include refid="search" />	
					<![CDATA[
			        GROUP BY  A.OUTS_CODE
			    )G ON A.OUTS_CODE = G.OUTS_CODE
			    WHERE 1=1
			    ]]>
				     <include refid="search" />	
					<![CDATA[
			)A
			ORDER BY CUST_CODE, CLAS_CODE,JOBN_NUMB,ROW_NUM, GBN,DEND_AMTX DESC

					    
			]]>
	</select>
	
	<!-- 
	/***********************************************************************************
     작  성  자 : 최제현
     작  성  일 : 2017.01.23
     내      용 : 제작 결산관리 실적집계표(외주처별 개인별 상세)
     파라미터   :
   /***********************************************************************************
     -->
	<select id="PRPA0100.PRINT00" parameterClass="hashmap" resultClass="hashmap">
	        <![CDATA[
	        
	        SELECT
		        #YEAR_MONT1# YEAR_MONT1
				,#YEAR_MONT2# YEAR_MONT2
				, CUST_NAME
				, CLAS_NAME
				, JOBN_NUMB
				, JOBN_NAME
				, OUTS_CODE
				, OUTS_NAME
				, SUBX_NAME AS ITEM_NAME
				, NVL(SUJU_AMTX,0) AS SUJU_AMTX
				, NVL(COMM_SION,0) AS COMM_SION
				, NVL(DEND_AMTX,0) AS DEND_AMTX
				, ROW_NUM
				FROM (
				    SELECT
				    '1' GBN
				    , B.CUST_CODE 
				    , C.CUST_NAME
				    , D.CLAS_CODE
				    , D.CLAS_NAME
				    , A.JOBN_NUMB
				    , B.JOBN_NAME
				    , F.SUBX_NAME
				    , A.OUTS_CODE
				    , E.CUST_NAME AS OUTS_NAME 
				    , SUM(A.SUJU_AMTX) AS SUJU_AMTX
				    , SUM(A.DEND_AMTX) - SUM(A.SUJU_AMTX) AS COMM_SION
				    , SUM(A.DEND_AMTX) AS DEND_AMTX
				    , G.ROW_NUM
				    FROM PD_SUJUMD A
				    LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
				    LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
				    LEFT JOIN (
				        SELECT CLAS_CODE, CLAS_NAME FROM PD_CODEXM
				        WHERE 1=1
				        AND MEMO_FILD = 'TYPE'
				        GROUP BY CLAS_CODE, CLAS_NAME
				    )D ON B.MEDA_CODE = D.CLAS_CODE
				    LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
				    LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
				    LEFT JOIN (
				        SELECT 
				         A.OUTS_CODE
				        , SUM(A.DEND_AMTX) AS DEND_AMTX
				        , ROW_NUMBER() OVER (ORDER BY  SUM(A.DEND_AMTX) DESC) AS ROW_NUM
				        FROM PD_SUJUMD A
				        LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
				        LEFT JOIN TA_CUSTXM C ON B.CUST_CODE = C.CUST_CODE
				        LEFT JOIN (
				            SELECT CLAS_CODE, CLAS_NAME FROM PD_CODEXM
				            WHERE 1=1
				            AND MEMO_FILD = 'TYPE'
				            GROUP BY CLAS_CODE, CLAS_NAME
				        )D ON B.MEDA_CODE = D.CLAS_CODE
				        LEFT JOIN TA_CUSTXM E ON A.OUTS_CODE = E.CUST_CODE
				        LEFT JOIN PD_ITEMXD F ON A.ITEM_CODE = F.ITEM_CODE
				        WHERE 1=1
				            ]]>
					     <include refid="search" />	
						<![CDATA[
				        GROUP BY  A.OUTS_CODE
				    )G ON A.OUTS_CODE = G.OUTS_CODE
				    WHERE 1=1
				    ]]>
					     <include refid="search" />	
						<![CDATA[
				    GROUP BY B.CUST_CODE, C.CUST_NAME, D.CLAS_NAME, D.CLAS_CODE, A.JOBN_NUMB
				    , B.JOBN_NAME, A.OUTS_CODE, E.CUST_NAME,F.SUBX_NAME, G.ROW_NUM
				)A
				ORDER BY CUST_CODE, CLAS_CODE,JOBN_NUMB,ROW_NUM, GBN,DEND_AMTX DESC    
			]]>
	</select>
	
