	<select id="PRPA0100.PRINT01" parameterClass="hashmap" resultClass="hashmap">
	        <![CDATA[ 
						SELECT
						#YEAR_MONT1# YEAR_MONT1
						,#YEAR_MONT2# YEAR_MONT2
						FROM DUAL
			]]>
	</select>
	
</sqlMap>
