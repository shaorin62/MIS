				, GUBUN					/* 매체구분 */
				, MEDX_FLAG			/* 매체구분코드 */
				, YEAR_MNTH			/* 년월 */
				, MEDX_CODE			/* 매체사코드 */
				, BIZR_NUMB			/* 사업자번호 */
				, MEDX_NAME			/* 매체사명 */
				, COMI_FILD			/* 금액 */
				, COMI_VAT				/* 부가세액 */
				, COMI_SUM			/* 합계금액 */
				, MEMO_FILD			/* 내용 */
				FROM (
				    SELECT
				    ROWNUM AS IDXX_SEQX
				    , '인쇄' AS GUBUN
				    , MEDX_FLAG
				    , YEAR_MNTH
				    , MEDX_CODE
				    , BIZR_NUMB
				    , MEDX_NAME
				    , SUM(COMI_FILD) AS COMI_FILD
				    , SUM(COMI_VAT) AS COMI_VAT
				    , SUM(COMI_FILD) + SUM(COMI_VAT)  AS COMI_SUM
				    , SUBSTR(XMLAGG(XMLELEMENT(X,'/',CUST_NAME) ORDER BY CUST_NAME).Extract('//text()'),2) || ' ' || SUBSTR(YEAR_MNTH,5,2) || '월 광고대행 수수료' AS MEMO_FILD		
				    FROM (
				        SELECT					
				        YEAR_MNTH
				        , A.MEDX_FLAG
				        , A.MEDX_CODE			
				        , C.BIZR_NUMB			
				        , C.CUST_NAME AS MEDX_NAME				
				        , SUM(COMI_FILD)  AS COMI_FILD		
				        , (SUM(COMI_FILD) * 0.1) AS COMI_VAT
				        , A.CUST_CODE
				        , B.CUST_NAME
				        FROM MD_PTMDXM A	
				        LEFT JOIN TA_CUSTXM B ON A.CUST_CODE = B.CUST_CODE
				        LEFT JOIN TA_CUSTXM C ON A.MEDX_CODE = C.CUST_CODE 
				        WHERE 1=1
				        ]]>
				     	<include refid="search" />	
						<![CDATA[
				        AND A.COMI_FILD IS NOT NULL 
				        AND A.COMI_FILD != 0
				        AND A.TRTR_NUMB IS NOT NULL 
				        GROUP BY YEAR_MNTH, A.CUST_CODE, B.CUST_NAME
				        ,A.MEDX_CODE,C.CUST_NAME ,C.BIZR_NUMB, A.MEDX_FLAG
				    )A
				    GROUP BY YEAR_MNTH, MEDX_CODE
				    , BIZR_NUMB, MEDX_NAME, ROWNUM, MEDX_FLAG
				    UNION ALL
				    SELECT
				    ROWNUM AS IDXX_SEQX
				    , '전파' AS GUBUN
				    , MEDX_FLAG
				    , YEAR_MNTH
				    , MEDX_CODE
				    , BIZR_NUMB
				    , MEDX_NAME
				    , SUM(COMI_FILD) AS COMI_FILD
				    , SUM(COMI_VAT) AS COMI_VAT
				    , SUM(COMI_FILD) + SUM(COMI_VAT)  AS COMI_SUM
				    , SUBSTR(XMLAGG(XMLELEMENT(X,'/',CUST_NAME) ORDER BY CUST_NAME).Extract('//text()'),2) || ' ' || SUBSTR(YEAR_MNTH,5,2) || '월 광고대행 수수료' AS MEMO_FILD		
				    FROM (
				        SELECT					
				        YEAR_MNTH		
						, A.MEDX_FLAG
				        , A.MEDX_CODE			
				        , C.BIZR_NUMB			
				        , C.CUST_NAME AS MEDX_NAME				
				        , SUM(COMI_FILD)  AS COMI_FILD		
				        , (SUM(COMI_FILD) * 0.1) AS COMI_VAT
				        , A.CUST_CODE
				        , B.CUST_NAME
				        FROM MD_ETMDXM A	
				        LEFT JOIN TA_CUSTXM B ON A.CUST_CODE = B.CUST_CODE
				        LEFT JOIN TA_CUSTXM C ON A.MEDX_CODE = C.CUST_CODE 
				        WHERE 1=1
				        ]]>
				     	<include refid="search" />	
						<![CDATA[
				        AND A.COMI_FILD IS NOT NULL 
				        AND A.COMI_FILD != 0
				        AND A.TRTR_NUMB IS NOT NULL 
				        GROUP BY YEAR_MNTH, A.CUST_CODE, B.CUST_NAME
				        ,A.MEDX_CODE,C.CUST_NAME ,C.BIZR_NUMB, A.MEDX_FLAG
				    )A
				    GROUP BY YEAR_MNTH, MEDX_CODE
				    , BIZR_NUMB, MEDX_NAME, ROWNUM, MEDX_FLAG 
				    UNION ALL
				    SELECT
				    ROWNUM AS IDXX_SEQX
				    , '케이블' AS GUBUN
				    , MEDX_FLAG
				    , YEAR_MNTH
				    , MEDX_CODE
				    , BIZR_NUMB
				    , MEDX_NAME
				    , SUM(COMI_FILD) AS COMI_FILD
				    , SUM(COMI_VAT) AS COMI_VAT
				    , SUM(COMI_FILD) + SUM(COMI_VAT)  AS COMI_SUM
				    , SUBSTR(XMLAGG(XMLELEMENT(X,'/',CUST_NAME) ORDER BY CUST_NAME).Extract('//text()'),2) || ' ' || SUBSTR(YEAR_MNTH,5,2) || '월 광고대행 수수료' AS MEMO_FILD		
				    FROM (
				        SELECT					
				        YEAR_MNTH		
						, A.MEDX_FLAG
				        , A.MEDX_CODE			
				        , C.BIZR_NUMB			
				        , C.CUST_NAME AS MEDX_NAME				
				        , SUM(COMI_FILD)  AS COMI_FILD		
				        , (SUM(COMI_FILD) * 0.1) AS COMI_VAT
				        , A.CUST_CODE
				        , B.CUST_NAME
				        FROM MD_CTMDXM A	
				        LEFT JOIN TA_CUSTXM B ON A.CUST_CODE = B.CUST_CODE
				        LEFT JOIN TA_CUSTXM C ON A.MEDX_CODE = C.CUST_CODE 
				        WHERE 1=1
				        ]]>
				     	<include refid="search" />	
						<![CDATA[
				        AND A.COMI_FILD IS NOT NULL 
				        AND A.COMI_FILD != 0
				        AND A.TRTR_NUMB IS NOT NULL 
				        GROUP BY YEAR_MNTH, A.CUST_CODE, B.CUST_NAME
				        ,A.MEDX_CODE,C.CUST_NAME ,C.BIZR_NUMB, A.MEDX_FLAG
				    )A
				    GROUP BY YEAR_MNTH, MEDX_CODE
				    , BIZR_NUMB, MEDX_NAME, ROWNUM, MEDX_FLAG
				    UNION ALL
				    SELECT
				    ROWNUM AS IDXX_SEQX
				    , '인터넷' AS GUBUN
				    , MEDX_FLAG
				    , YEAR_MNTH
				    , MEDX_CODE
				    , BIZR_NUMB
				    , MEDX_NAME
				    , SUM(COMI_FILD) AS COMI_FILD
				    , SUM(COMI_VAT) AS COMI_VAT
				    , SUM(COMI_FILD) + SUM(COMI_VAT)  AS COMI_SUM
				    , SUBSTR(XMLAGG(XMLELEMENT(X,'/',CUST_NAME) ORDER BY CUST_NAME).Extract('//text()'),2) || ' ' || SUBSTR(YEAR_MNTH,5,2) || '월 광고대행 수수료' AS MEMO_FILD		
				    FROM (
				        SELECT					
				        YEAR_MNTH	
				        , A.MEDX_FLAG
				        , A.MEDX_CODE			
				        , C.BIZR_NUMB			
				        , C.CUST_NAME AS MEDX_NAME				
				        , SUM(COMI_FILD)  AS COMI_FILD		
				        , (SUM(COMI_FILD) * 0.1) AS COMI_VAT
				        , A.CUST_CODE
				        , B.CUST_NAME
				        FROM MD_ITMDXM A	
				        LEFT JOIN TA_CUSTXM B ON A.CUST_CODE = B.CUST_CODE
				        LEFT JOIN TA_CUSTXM C ON A.MEDX_CODE = C.CUST_CODE 
				        WHERE 1=1
				        ]]>
				     	<include refid="search" />	
						<![CDATA[
				        AND A.COMI_FILD IS NOT NULL 
				        AND A.COMI_FILD != 0
				        AND A.TRTR_NUMB IS NOT NULL 
				        GROUP BY YEAR_MNTH, A.CUST_CODE, B.CUST_NAME
				        ,A.MEDX_CODE,C.CUST_NAME ,C.BIZR_NUMB, A.MEDX_FLAG
				    )A
				    GROUP BY YEAR_MNTH, MEDX_CODE
				    , BIZR_NUMB, MEDX_NAME, ROWNUM, MEDX_FLAG
				    UNION ALL
				    SELECT
				    ROWNUM AS IDXX_SEQX
				    , '옥외' AS GUBUN
				    , MEDX_FLAG
				    , YEAR_MNTH
				    , MEDX_CODE
				    , BIZR_NUMB
				    , MEDX_NAME
				    , SUM(COMI_FILD) AS COMI_FILD
				    , SUM(COMI_VAT) AS COMI_VAT
				    , SUM(COMI_FILD) + SUM(COMI_VAT)  AS COMI_SUM
				    , SUBSTR(XMLAGG(XMLELEMENT(X,'/',CUST_NAME) ORDER BY CUST_NAME).Extract('//text()'),2) || ' ' || SUBSTR(YEAR_MNTH,5,2) || '월 광고대행 수수료' AS MEMO_FILD		
				    FROM (
				        SELECT					
				        YEAR_MNTH		
						, A.MEDX_FLAG
				        , A.MEDX_CODE			
				        , C.BIZR_NUMB			
				        , C.CUST_NAME AS MEDX_NAME				
				        , SUM(COMI_FILD)  AS COMI_FILD		
				        , (SUM(COMI_FILD) * 0.1) AS COMI_VAT
				        , A.CUST_CODE
				        , B.CUST_NAME
				        FROM MD_ODMDXM A	
				        LEFT JOIN TA_CUSTXM B ON A.CUST_CODE = B.CUST_CODE
				        LEFT JOIN TA_CUSTXM C ON A.MEDX_CODE = C.CUST_CODE 
				        WHERE 1=1
				        ]]>
				     	<include refid="search" />	
						<![CDATA[
				        AND A.COMI_FILD IS NOT NULL 
				        AND A.COMI_FILD != 0
				        AND A.TRTR_NUMB IS NOT NULL 
				        GROUP BY YEAR_MNTH, A.CUST_CODE, B.CUST_NAME
				        ,A.MEDX_CODE,C.CUST_NAME ,C.BIZR_NUMB, A.MEDX_FLAG
				    )A
				    GROUP BY YEAR_MNTH, MEDX_CODE
				    , BIZR_NUMB, MEDX_NAME, ROWNUM, MEDX_FLAG
				)A
				WHERE 1=1
				ORDER BY   GUBUN, IDXX_SEQX ASC





	]]>
	</select>

	<!-- 
		작  성  자 :  최제현
		작  성  일 : 2016.11.21(표준화 완료)
		내      용  : 매체사목록 조회
		파라미터  : 
	-->
	<select id="MACA0010.SEARCH01" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[ 
		SELECT                                         /* MDMA0030.SEARCH01 매체사목록 가져오기 */
                A.CUST_CODE								/* 매체사코드 */
                , A.CUST_NAME                                 /* 거래처명 */
                , A.CUST_NAME AS CLIENT_NAME    /* 거래처명 */
                , ' ' AS USEX_CODE                          /* NULL */
                FROM TA_CUSTXM A 
                LEFT JOIN TA_CUSTXD B ON A.CUST_CODE = B.CUST_CODE
                WHERE 1=1
                AND B.CUCL_CODE = 'FA260120'
	]]>
	</select>
	
	
		<!-- 
		작  성  자 :  오세훈
		작  성  일 : 2016.11.30(표준화 완료)
		내      용  : 입금표 출력을 위한 조회
		파라미터  : 
