		, BIZR_NUMB				/* 사업자번호 */
		, MEDX_NAME				/* 매체사명 */
		, COMI_FILD				/* 금액 */
		, COMI_VAT					/* 부가세액*/
		, COMI_SUM				/* 합계금액*/
		, MEMO_FILD				/* 내용 */
		FROM (
		    SELECT
		    ROWNUM AS IDXX_SEQX
		    , '인쇄' AS GUBUN
		    , MEDX_FLAG
		    , YEAR_MNTH
		    , MEDX_CODE
		    , BIZR_NUMB
		    , MEDX_NAME
		    , SUM(COMI_FILD) AS COMI_FILD
		    , SUM(COMI_VAT) AS COMI_VAT
