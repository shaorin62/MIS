		        AND A.TRTR_NUMB IS NOT NULL 
		        GROUP BY YEAR_MNTH, A.CUST_CODE, B.CUST_NAME
		        ,A.MEDX_CODE,C.CUST_NAME ,C.BIZR_NUMB, A.MEDX_FLAG
		    )A
		    GROUP BY YEAR_MNTH, MEDX_CODE
		    , BIZR_NUMB, MEDX_NAME, ROWNUM, MEDX_FLAG
		    UNION ALL
		    SELECT
		    ROWNUM AS IDXX_SEQX
