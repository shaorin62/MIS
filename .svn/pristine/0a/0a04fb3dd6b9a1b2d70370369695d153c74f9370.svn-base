/*-----------------------+
 |  팝업 Setting  |
 +------------------------*/
this.fn_HelpDialoge = function (obj, sFromDs, sOldValue, sColumnID) {

	if(obj.readonly == true) return;
	
	var arrParam = new Array();

	if ((obj.name == "imgHelpSHREMPL_NUMB") || (obj.name == "edtSHREMPL_NAME")) { // 조회조건 - 사원정보조회(Popup)
		arrParam[0] = "HRM0001";
		arrParam[1] = "";
		arrParam[2] = this.fnc_Trim(this.edtSHREMPL_NAME.value);
		arrParam[3] = "N";
		arrParam[4] = "edtSHREMPL_NAME";
		arrParam[5] = "edtSHREMPL_NUMB,edtSHREMPL_NAME";
		arrParam[6] = "0,1";
		this.fnc_HelpDialogeAction(this, obj, arrParam);
		
		if ( this.fnc_Length(this.edtSHREMPL_NUMB.value) > 0 ) this.fn_Search();
		
	}

	if (obj.name == "grdHR_SCHLRQ") {
		if (this.fnc_getGridHelpColName(obj, sFromDs) == "SCHL_CDNM") { // 입력 그리드 - 학력
			arrParam[0] = "TMM1000";
			arrParam[1] = "SCHL_CODE";
			arrParam[2] = "SCHL_CDNM";
			arrParam[3] = "N";
			arrParam[4] = "grdHR_SCHLRQ";
			arrParam[5] = "SCHL_CODE,SCHL_CDNM";
			arrParam[6] = "0,1";
			arrParam[7] = sFromDs;
			arrParam[8] = sOldValue;
			
			this.fnc_HelpDialogeAction(this, obj, arrParam);
		
		} else if (this.fnc_getGridHelpColName(obj, sFromDs) == "MAJR_CDNM") { // 입력 그리드 - 전공
			arrParam[0] = "TMM1000";
			arrParam[1] = "MAJR_CODE";
			arrParam[2] = "MAJR_CDNM";
			arrParam[3] = "N";
			arrParam[4] = "grdHR_SCHLRQ";
			arrParam[5] = "MAJR_CODE,MAJR_CDNM";
			arrParam[6] = "0,1";
			arrParam[7] = sFromDs;
			arrParam[8] = sOldValue;
			
			this.fnc_HelpDialogeAction(this, obj, arrParam);
		}
	}

}

/*------------------+
 |  변경요청내용 추가  |
 +------------------*/
this.fn_btnAddRow_OnClick = function (obj:Button, e:ClickEventInfo) {

	if (this.fnc_Length(this.edtSHREMPL_NUMB.value) < 1 || this.dsHR_MSTCRQ.getRowCount() < 1) {
		this.fnc_Message("HRM017");
		return false;
	}
	
	var iRow = this.dsHR_SCHLRQ.addRow();

	this.dsHR_SCHLRQ.setColumn(iRow, "EMPL_NUMB", this.dsHR_MSTCRQ.getColumn(this.dsHR_MSTCRQ.rowposition, "EMPL_NUMB"));
	this.dsHR_SCHLRQ.setColumn(iRow, "CHRQ_DATE", this.dsHR_MSTCRQ.getColumn(this.dsHR_MSTCRQ.rowposition, "CHRQ_DATE"));
	this.dsHR_SCHLRQ.setColumn(iRow, "CHRQ_SEQN", this.dsHR_MSTCRQ.getColumn(this.dsHR_MSTCRQ.rowposition, "CHRQ_SEQN"));
	this.dsHR_SCHLRQ.setColumn(iRow, "CHDV_CODE", "I");
	
	this.grdHR_SCHLRQ.setFocus();
	this.grdHR_SCHLRQ.setCellPos(this.fnc_GridColumnIndex(this.grdHR_SCHLRQ, "SCDN_CODE"));
	
	return;
}

/*------------------+
 |  변경요청내용 삭제  |
 +------------------*/
this.fn_btnDelRow_OnClick = function (obj:Button, e:ClickEventInfo) {

	if (this.fn_DeleteCheck("dsHR_SCHLRQ")) {
	
		//신규행일경우 삭제처리하고 신규행이 아닐경우 삭제요청상태로 변경
		if(this.dsHR_SCHLRQ.getColumn(this.dsHR_SCHLRQ.rowposition, "CHDV_CODE") == "I"){
			this.dsHR_SCHLRQ.deleteRow(this.dsHR_SCHLRQ.rowposition);
			
		} else {
			this.dsHR_SCHLRQ.setColumn(this.dsHR_SCHLRQ.rowposition, "CHDV_CODE", "D");

		}
		
	}
	this.grdHR_SCHLRQ.setFocus();

}

/*----------------------+
 |  변경요청 버튼 클릭 時 |
 +----------------------*/
this.fn_btnChangeReq = function(obj:Button, e:ClickEventInfo) {
	
	var row = this.dsHR_MSTCRQ.rowposition;
	
	if (row < 0) {
		this.fnc_Message("TMM010");   //Message 처리 후
		this.grdHR_MSTCRQ.setFocus();
		return;
	}

	/*
		//증빙파일이 있는 지 체크
		if (dtlTM_ATFILE.getRowCount() < 1) {
			//해당 변경내역에 대한 증빙서류를 첨부하여야 합니다!
			this.fnc_Message("HRM044");
			btnAddFile.focus();
			return false;
		}
	*/
			
	if ( this.fnc_Message("HRM015")) {
		this.dsHR_MSTCRQ.setColumn(row, "CHRC_CODE", "0");
		this.sSAVE_CODE = "R";	//변경요청
		this.fn_Save();
	}
}

/*----------------------+
 |  요청취소 버튼 클릭 時 |
 +----------------------*/
this.fn_btnReqCancel_OnClick = function(obj:Button, e:ClickEventInfo) {
	
	var row = this.dsHR_MSTCRQ.rowposition;
	
	if (row < 0) {
		this.fnc_Message("TMM010");   //Message 처리 후
		this.grdHR_MSTCRQ.setFocus();
		return;
	}

	if ( this.fnc_Message("HRM016")) {
		this.dsHR_MSTCRQ.setColumn(row, "CHRC_CODE", "S");
		this.sSAVE_CODE = "C";	//요청취소
		this.fn_Save();
	}  
}

/*-------------------------+
 |  DATASET COLUMN 변경 후 |
 +-------------------------*/
this.dsHR_SCHLRQ_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo) {
	
	if (e.columnid == "SCHL_CDNM" || e.columnid == "MAJR_CDNM"){
		this.fn_HelpDialoge(this.grdHR_SCHLRQ, "YES" ,e.oldvalue, e.columnid);
	} else if ( e.columnid == "HISL_YSNO" ) {
		this.dsHR_SCHLRQ.set_enableevent(false);
		for ( var i=0; i<this.dsHR_SCHLRQ.getRowCount(); i++ ) {
			if ( i == e.row ) {
				this.dsHR_SCHLRQ.setColumn(i, "HISL_YSNO", "1"); 
			} else {
				this.dsHR_SCHLRQ.setColumn(i, "HISL_YSNO", "0");
			}
		}
		this.dsHR_SCHLRQ.set_enableevent(true);
	}

	var iRow = e.row;

	if (this.dsHR_SCHLRQ.getColumn(iRow, "CHDV_CODE") == "D") return;
 	
	var vSRAL_NUMB = this.dsHR_SCHLRQ.getColumn(iRow, "SRAL_NUMB");
	var iFindRow = this.dsHR_SCHCRR.findRow("SRAL_NUMB", vSRAL_NUMB)
	
	if (iFindRow < -1) {
		this.dsHR_SCHLRQ.setColumn(row, "CHDV_CODE", "I");
	
	} else {
		
		for(var i = 0; i < this.dsHR_SCHCRR.getColCount(); i++) {
			//원본데이터를 기준으로 비교하면 요청데이터의 변경구분 컬럼은 비교 안함.
			if (this.fnc_Quote(this.dsHR_SCHCRR.getColumn(iFindRow, this.dsHR_SCHCRR.getColID(i))) != this.fnc_Quote(this.dsHR_SCHLRQ.getColumn(iFindRow, this.dsHR_SCHCRR.getColID(i)))){
				this.dsHR_SCHLRQ.setColumn(iRow, "CHDV_CODE", "U");
				return;
			}
			
		}
	}
}

this.dsHR_MSTCRQ_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo){
	if (this.fnc_DatasetChangeCheck("dsHR_SCHLRQ")) {
		if (this.fnc_Message("TMM023") == false)  { 
			return false;
		}else{
			return true;
		}
	}
}

