this.grd_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo) {
	
	if ( this.dsHR_MSTCRQ.getColumn(this.dsHR_MSTCRQ.rowposition, "CHRC_CODE") == "S"){
		if ( obj.id == "grdHR_SCHLRQ" ) {
			if ( this.fnc_getGridHelpColName(obj, e.cell) == "HISL_YSNO") { // 입력 그리드 - 학력
				
				this.dsHR_SCHLRQ.set_enableevent(false);
				for ( var i=0; i<this.dsHR_SCHLRQ.getRowCount(); i++ ) {
					if ( i == e.row ) {
						this.dsHR_SCHLRQ.setColumn(i, "HISL_YSNO", "1"); 
					} else {
						this.dsHR_SCHLRQ.setColumn(i, "HISL_YSNO", "0");
					}
				}
				this.dsHR_SCHLRQ.set_enableevent(true);
			}
		}
	}
}

