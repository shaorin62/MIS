			this.SET_YEA_YEFAMI("EDUC", e.row);
			this.SET_YEA_INCDEC("EDUC");
			break;
			
		case "YEA_FAM_NUM009":			// 신용카드국세청
			this.SET_YEA_YEFAMI("CARD", e.row);
			this.SET_YEA_INCDEC("CARD");			
