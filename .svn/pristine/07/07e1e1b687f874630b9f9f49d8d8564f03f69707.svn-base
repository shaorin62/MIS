			this.SET_YEA_YEFAMI("CASH", e.row);
			this.SET_YEA_INCDEC("CASH");						
			break;
			
		case "YEA_FAM_NUM014":	// 전통시장국세청			
			this.SET_YEA_YEFAMI("MRKT", e.row);
			this.SET_YEA_INCDEC("MRKT");
			break;
			
		case "YEA_FAM_NUM015":	// 전통시장기타			
			this.SET_YEA_YEFAMI("MRKT", e.row);
			this.SET_YEA_INCDEC("MRKT");			
			break;
			
