			break;

		case "YEA_FAM_NUM006":	// 의료비기타
			this.SET_YEA_INCDEC("MEDI");
			break;
			
		case "YEA_FAM_STR010":		// 교육비구분코드	
			this.SET_YEA_YEFAMI("EDUC", e.row);
			this.SET_YEA_INCDEC("EDUC");
			break;
			
		case "YEA_FAM_NUM007":			// 교육비국세청
			this.SET_YEA_YEFAMI("EDUC", e.row);
			this.SET_YEA_INCDEC("EDUC");
			break;
			
