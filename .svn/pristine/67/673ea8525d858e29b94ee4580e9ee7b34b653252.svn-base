		arrParam[5] = "edtSHRDEPT_CODE,edtSHRDEPT_NAME"; 		
		arrParam[6] = "0,1"; 									
		
	} else if ((obj.name == "imgHelpSHREMPL_NUMB") || (obj.name == "edtSHREMPL_NAME")) { // 조회조건 - 사원정보조회(Popup)
