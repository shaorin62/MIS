/*------------------------------------+
 |  파일을 서버에 드랍하고 사용자에게 표시한다.
 +-------------------------------------*/
this.btnDROP_FILE_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{

	var sGBN = true;
	var sCONT_NUMB = "";
	var sCONT_NAME = "";
	var j = 0 ;
	//헤더를 돌면서 체크 된 내역에 파일 명을 입력 및 RPT 데이터 셋에 데이터 담아서 떨군다.
	for(var i = 0; i < this.dsPD_CONTXH.rowcount; i ++){
		if(this.fn_nullChk(this.dsPD_CONTXH.getColumn(i,"CHK")) == "1"){
			var ubiExportFilename = this.dsPD_CONTXH.getColumn(i,"CONT_NUMB") +  j; 
			
			if(sGBN){
				sCONT_NUMB = this.dsPD_CONTXH.getColumn(i,"CONT_NUMB");
				sCONT_NAME = this.dsPD_CONTXH.getColumn(i,"CONT_NAME");
				sGBN = false;
			}else{
				sCONT_NUMB += ',' + this.dsPD_CONTXH.getColumn(i,"CONT_NUMB");
				sCONT_NAME += ',' + this.dsPD_CONTXH.getColumn(i,"CONT_NAME");
			}
			
			this.dsPD_CONTXH.setColumn(i,"DROP_FILE", ubiExportFilename);
			
			j += 1
		}
	}
	
	//PDF 파일을 서버에 생성 시킨다.
	this.fn_SendEmailDrop(sCONT_NUMB, sCONT_NAME);
	
}

