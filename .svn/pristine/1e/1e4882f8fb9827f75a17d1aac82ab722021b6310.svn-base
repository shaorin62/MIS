	this.dsFS_ASSTXM.setColumn(iRow, "ACQU_CHNL", "MANU"); //취득경로(초기값은MANU : 수기등록)
	this.dsFS_ASSTXM.setColumn(iRow, "SAVG_VALU", 0); //잔존가액
	this.dsFS_ASSTXM.setColumn(iRow, "PRID_DPCT", 0); //내용년수
