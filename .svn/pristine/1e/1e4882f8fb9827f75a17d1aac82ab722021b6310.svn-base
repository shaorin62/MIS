		sReturnString += " ASST_NUMB=" + this.fnc_Quote(this.fnc_Trim(this.dsFS_ASSTXM.getColumn(this.dsFS_ASSTXM.rowposition, "ASST_NUMB"))); //자산번호
		

	} else if (sKind == "SEARCH02") {

