}

/*-----------------------------+
 |  05.삭제(제거) 버튼 클릭 時  |
 +------------------------------*/
this.fn_Delete = function (obj) {

	if (this.fn_DeleteCheck()) {
		this.dsFS_ASRPXM.deleteRow(this.dsFS_ASRPXM.rowposition);
	}
	this.grdFS_ASRPXM.setFocus();
}

/*-----------------------+
