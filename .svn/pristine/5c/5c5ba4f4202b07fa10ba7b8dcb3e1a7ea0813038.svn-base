	} else {
		this.aKeyValue[2] = 1; //신규생성은 1. PKG에서 생성함.
	}
	//trace("-----aKeyValue[0] : " + this.aKeyValue[0] + " aKeyValue[1] : " + this.aKeyValue[1] + " aKeyValue[2] : " + this.aKeyValue[2]);

