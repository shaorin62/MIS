	if (!this.fn_SearchItemCheck()) return;

	this.fnc_DatasetClear('dsFS_ASRPXM'); //기 조회 자료 초기화

	var sMethodName = "SEARCH00";
	var sInDataSet = "";
	var sOutDataSet = "dsFS_ASRPXM=dsFS_ASRPXM";
