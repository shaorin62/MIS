 +--------------------------------------------*/
this.dsFS_ASRPXM_OnColumnChanged = function (obj:Dataset, e:nexacro.DSColChangeEventInfo) {

	if (e.columnid == "REPR_YSNO") {
		if (e.newvalue == "1") {
			this.fn_ComponentEnable(true);
