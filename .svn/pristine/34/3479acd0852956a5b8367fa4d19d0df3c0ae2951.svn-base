this.fn_DeleteCheck = function (objds) {

    // TODO 전자결재 관련
//  var nRow = dsCommonCode_GRWS_CODE.getMatchedIndex("COMD_CODE", dsHR_MASTXM.getColumn(dsHR_MASTXM.rowposition, "GRWS_CODE"), true);
