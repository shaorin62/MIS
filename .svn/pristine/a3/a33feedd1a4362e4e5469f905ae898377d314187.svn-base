// 		this.fnc_SetReadonly(this.edtUSER_NAME, false);
// 		this.fnc_SetReadonly(this.edtDEPT_NAME, false);
// 		this.fnc_SetReadonly(this.imgHelpDEPT_CODE, false);
// 	}
// 
// 	this.fn_AuthGroupSearch();
