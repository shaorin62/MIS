 -->
	
	<!-- 
		작  성  자 : 최제현
		작  성  일 : 2016.10.14(표준화 완료)
		내      용 : 옥외 개별청약 전체복사 저장 처리
	-->
	<insert id="MDME0010P01.SAVE00" parameterClass="hashmap">
	<![CDATA[
		INSERT INTO MD_ODMDXM (		/* MDME0010P01.SAVE00 옥외 개별청약 전체복사 저장 */
					YEAR_MNTH		/*년월*/
				    ,SEQX_FILD			/*일련번호*/
				    ,CUST_CODE		/*광고주코드*/
				    ,CTIM_CODE		/*광고주팀코드*/
				    ,BRND_CODE		/*브랜드코드*/
				    ,MEDX_CODE		/*청구지코드*/
				    ,RMED_CODE		/*매체코드*/
				    ,APLY_DATE			/*적용일자*/
				    ,MATT_NAME		/*소재명*/
				    ,EMPL_CODE		/*사원코드*/
				    ,DEPT_CODE		/*부서코드*/
				    ,STAR_DATE		/*광고시작일*/
				    ,ENDD_DATE		/*광고종료일*/
				    ,AMNT_FILD		/*광고비*/
				    ,COMI_FILD			/*수수료*/
				    ,COMI_RATE		/*수수료율*/
				    ,TRTX_FLAG		/*위수탁부가세유무*/
				    ,CMTX_FLAG		/*수수료부가세유무*/
				    ,MEMO_FILD		/*비고*/
				    ,ODAD_FLAG		/*옥외광고구분*/
				    ,AORX_FLAG		/*AOR구분*/
				    ,INST_USID			/*입력자*/
				    ,INST_DATE			/*입력일*/
				    ,UPDT_USID		/*수정자*/
				    ,UPDT_DATE		/*수정일*/
		)SELECT #nYEAR_MNTH#/*년월*/
				    ,#nSEQX_FILD#	/*일련번호*/
				    ,CUST_CODE		/*광고주코드*/
				    ,CTIM_CODE		/*광고주팀코드*/
				    ,BRND_CODE		/*브랜드코드*/
				    ,MEDX_CODE		/*청구지코드*/
				    ,RMED_CODE		/*매체코드*/
				    ,APLY_DATE			/*적용일자*/
				    ,MATT_NAME		/*소재명*/
				    ,EMPL_CODE		/*사원코드*/
				    ,DEPT_CODE		/*부서코드*/
				    ,STAR_DATE		/*광고시작일*/
				    ,ENDD_DATE		/*광고종료일*/
				    ,AMNT_FILD		/*광고비*/
				    ,COMI_FILD			/*수수료*/
				    ,COMI_RATE		/*수수료율*/
				    ,TRTX_FLAG		/*위수탁부가세유무*/
				    ,CMTX_FLAG		/*수수료부가세유무*/
				    ,MEMO_FILD		/*비고*/
				    ,ODAD_FLAG		/*옥외광고구분*/
				    ,AORX_FLAG		/*AOR구분*/
				    ,INST_USID			/*입력자*/
				    ,TO_CHAR(SYSDATE,'YYYYMMDD')			/*입력일*/
				    ,UPDT_USID		/*수정자*/
				    ,UPDT_DATE		/*수정일*/
		FROM MD_ODMDXM
		WHERE YEAR_MNTH = #oYEAR_MNTH#
		AND SEQX_FILD=#oSEQX_FILD#
	]]>
	</insert>
	
		<!-- 
		작  성  자 : 최제현
		작  성  일 : 2016.11.16(표준화 완료)
		내      용  : 기존 년월 로우의 개수를 샌다
		파라미터  : 
	-->
	<select id="MDME0010P01.COUNTSEQX_FILD" parameterClass="hashmap" resultClass="int">
	<![CDATA[ 
		SELECT  /*  MDMA0030P01.COUNTSEQX_FILD 기존년월 데이터의 개수  */
				COUNT(SEQX_FILD)		/* 일련번호 */
		FROM MD_ODMDXM
		WHERE YEAR_MNTH	=	#oYEAR_MNTH# /* 년월 */
	]]>
	</select>
	
	<!-- 
		작  성  자 : 최제현
		작  성  일 : 2016.11.16(표준화 완료)
		내      용  : 신규 일련번호 코드 채번
		파라미터  : 
	-->
	<select id="MDME0010P01.NEW_SEQX_FILD" parameterClass="hashmap" resultClass="String">
	<![CDATA[ 
		SELECT  /*  MDMA0030P01.NEW_SEQX_FILD 신규 일련번호 채번  */
				NVL(MAX(TO_NUMBER(SEQX_FILD)),0)+1		/* 신규 일련번호 */
		FROM MD_ODMDXM
		WHERE YEAR_MNTH	=	#nYEAR_MNTH# /* 년월 */
	]]>
	</select>
	
</sqlMap>