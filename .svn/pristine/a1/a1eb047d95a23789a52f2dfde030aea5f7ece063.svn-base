{
	if(e.itemvalue == "20"){  //국외출장이면 해외시장개척비 활성화
	
		this.mae_RCPX_DNSP.enable = true; // 자료수집비
		this.mae_RCPX_PNSP.enable = true; // 접대비
	
	} else {
		
	    if(e.itemvalue == "30") {
			this.calSHRBSRP_DATE2.value = this.calSHRBSRP_DATE2.value;
	    }
	    
