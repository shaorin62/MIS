<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="COMHELPPOPUP">

<select id="COMHELPPOPUP.HelpFormDesign" parameterClass="hashmap" resultClass="hashmap">
<![CDATA[
	 SELECT  /* COMHELPPOPUP.HelpFormDesign [공통] Help팝업 Form디자인 조회 */
	         POPP_CODE   /*팝업코드        */
	        ,POPP_NAME   /*팝업명         */
	        ,POPP_CNTT   /*팝업정보        */
	        ,OWNE_NAME   /*소유자         */
	        ,AULU_YSNO   /*자동조회여부      */
	        ,CDRI_YSNO   /*조회필수입력여부    */
	        ,SERS_GRDC   /*조회결과그리드내용  */
	        ,SECH_SYNX   /*조회구분        */
	        ,FROM_SYNT   /*조회테이블       */
	        ,WHRE_SYNT   /*조회조건        */
	        ,WHRE_ADDX   /*조건추가        */
	        ,USYN_SYNX   /*사용여부구문      */
	        ,ADDT_SQLC   /*추가구문        */
	        ,SORT_SYNX   /*정렬구문        */
	        ,SECN_CBCT   /*조회조건콤보내용   */
	        ,POPP_SIZW   /* 팝업크기너비 */
            ,POPP_SIZH   /* 팝업크기높이 */
	  FROM TM_POPUPX    
	  WHERE POPP_CODE = #sSEARCHKIND#
]]>
</select>

<select id="COMHELPPOPUP.HelpDataDesign" parameterClass="hashmap" resultClass="hashmap">
<![CDATA[
	 SELECT  /* COMHELPPOPUP.HelpDataDesign [공통] Help팝업 dataDesign 조회 */
	         POPP_CODE   /*팝업코드        */
	        ,POPP_NAME   /*팝업명         */
	        ,POPP_CNTT   /*팝업정보        */
	        ,OWNE_NAME   /*소유자         */
	        ,AULU_YSNO   /*자동조회여부      */
	        ,CDRI_YSNO   /*조회필수입력여부    */
	        ,SERS_GRDC   /*조회결과그리드내용  */
	        ,SECH_SYNX   /*조회구분        */
	        ,FROM_SYNT   /*조회테이블       */
	        ,WHRE_SYNT   /*조회조건        */
	        ,WHRE_ADDX   /*조건추가        */
	        ,USYN_SYNX   /*사용여부구문      */
	        ,ADDT_SQLC   /*추가구문        */
	        ,SORT_SYNX   /*정렬구문        */
	        ,SECN_CBCT   /*조회조건콤보내용   */
	  FROM TM_POPUPX    
	  WHERE POPP_CODE = #sSEARCHKIND#
]]>
</select>

