Tab.styTopMenuTabControl
{
	accessibility 		: enable all '' '' '' ;
	background		: transparent ;
	border 			: 0 none #808080ff ;
	bordertype		: normal 0 0 ;
	buttonbackground : URL('theme://imagesNex/topframe/top_btn_bg_blank.gif') stretch ;
	
	buttonborder 		: 0 none #808080ff ;
	buttonbordertype	: normal 0 0 ;
	buttongradation		: none 0,0 white 100,100 black ;
	buttonpadding		: 5 10 5 10 ;
	color	: #565656ff ;
	cursor	: pointer ;
	font : bold 11 돋움 ;
	gradation	: none 0,0 white 100,100 black ;
	showextrabutton : false ;
	
	
}Tab.styTopMenuTabControl:enabled
{
	buttonbackground 	: #transparent URL('theme://imagesNex/topframe/top_btn_bg_blank.gif') stretch ;
	color 			: #565656ff;
	buttonpadding : 5 10 5 10 ;
	
	border 			: 0 none #808080ff ;
	bordertype		: normal 0 0 ;
	buttonborder 		: 0 none #808080ff ;
	buttonbordertype	: normal 0 0 ;
	buttongradation		: none 0,0 white 100,100 black ;
	gradation		: none 0,0 white 100,100 black ;
}

