/************************************************
  tree menu grid작업
*************************************************/

Grid.styGrid02
{
	align 			: left middle ;
	background		: white ;
	border 			: 0 none #dcdcdcff, 1 solid #dcdcdcff, 1 solid #dcdcdcff, 1 solid #dcdcdcff ;
	color			: #3d3d3dff ;
	font 			: 9 dotum ;
	line 			: 0 none #c8d0deff ;
	padding 		: 3 3 3 3 ;
	treeclosebuttonimage	: URL('theme://imagesInbus/ico_treeclose.png');
	treecollapseimage	: URL('theme://imagesInbus/ico_treecollapse.png');
	treeexpandimage		: URL('theme://imagesInbus/ico_treeexpand.png');
	treeitemimage		: URL('theme://imagesInbus/ico_treeitem.png');
	treelinetype		: 1 dotted #a5b6b6ff;
	treeopenbuttonimage	: URL('theme://imagesInbus/ico_treeopen.png');

}Grid.styGrid02>#head
{
	background 		: transparent ;
	border 			: 0 none #808080 ;
	cellbackground 		: @gradation ;
	cellbackground2 	: @gradation ;
	cellcolor 		: black ;	
	cellcolor2 		: black ;	
	cellfont 		: bold 9 dotum ;	
	cellline 		: 1 solid #b7b7b7ff ;	
	celllinetype 		: exvert ;	
	cellpadding 		: 0 3 0 3 ;	
	selectcolor 		: midnightblue ;	
	selectfont 		: bold 9 dotum ;	
	cellalign 		: center middle ;	
	cellgradation 		: linear 0,0 #eaeaeaff 0,100 #fdfdfdff ;	
}
Grid.styGrid02>#body
{
	background 		: white ;	
	border 			: 0 none #808080  ;	
	cellalign 		: center middle ;	
	cellbackground 		: white ;
	cellbackground2 	: white 0ff 0ff ;	
	cellcolor 		: #424b5cff ;	
	cellfont 		: 9 dotum ;	
	cellline 		: 1 solid #c8d0deff ;	
	celllinetype 		: exvert ;	
	cellpadding 		: 1 3 1 3 ;
	selectbackground 	: #eff5ffff 0fb 0fb ;	
	selectcolor 		: dodgerblue ;	
	selectfont 		: bold 9 dotum ;	
	selectline 		: 0 none #c8d0deff ;
	selectlinetype 		: exvert ;
	cellcolor2 		: #424b5cff ;	
}
Grid.styGrid02>#body:mouseover
{
	border 			: 0 none #808080  ;
	cellbackground 		: white ;	
	cellbackground2 	: #eff5ffff 0ff ;	
	cellcolor 		: #424b5cff ;	
	cellcolor2 		: #424b5cff ;
	selectline 		: 1 solid #c8d0deff ;	
}
Grid.styGrid02>#summ
{
	background 		: white ;	
	border 			: 0 none #808080  ;	
	cellbackground 		: #ffef99ff  ;	
	cellbackground2 	: #ffef99ff ;
	cellcolor 		: #424b5cff ;
	cellcolor2 		: #424b5cff ;
	cellline 		: 1 solid #808080ff ;		
}
Grid.styGrid02>#summary
{
	background 		: white ;	
	border 			: 0 none #808080  ;	
	cellbackground 		: #ffef99ff  ;	
	cellbackground2 	: #ffef99ff ;
	cellcolor 		: #424b5cff ;
	cellcolor2 		: #424b5cff ;
	cellline 		: 1 solid #808080ff ;	
}
Grid.styGrid02>#resizebutton
{
	background		: #ffffffff;
	border			: 1 solid #c8d0deff, 0 none, 0 none, 1 solid #c8d0deff;
	bordertype		: normal 0 0;
	color 			: #424b5cff ;		
}
Grid.styGrid02>#hscrollbar
{
	
}
Grid.styGrid02>#vscrollbar
{
	
}




