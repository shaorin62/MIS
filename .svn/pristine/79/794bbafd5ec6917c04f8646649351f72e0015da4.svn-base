	if ((this.fnc_DatasetChangeCheck("dsPD_ESTIMH")) && (this.fnc_Message("TMM023") == false) ) {
		return false;
	}
	if ((this.fnc_DatasetChangeCheck("dsPD_ESTIMD")) && (this.fnc_Message("TMM023") == false) ) {
		return false;
	}

	return true;
	// 모든 조회 조건이 입력 되면 True를 반환 한다.
}

/*------------------+
