	if (sMethodName == "SEARCH00") {
		this.fnc_Information(this.tabXM.tab0.stInformation, this.dsMD_TCALXM.getCaseCount("dataset.getRowLevel(currow)==0"), "SHR");
		this.fnc_getcust_codemail();
		this.fnc_getmedx_codemail();
