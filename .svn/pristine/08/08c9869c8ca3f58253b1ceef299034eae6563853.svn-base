	<!-- 
		작  성  자 :  최제현
		작  성  일 : 2016.10.19(표준화 완료)
		내      용  : 인터넷 개별청약 조회
		파라미터  : YEAR_MNTH : 년월, CUST_CODE : 광고주코드, BRND_CODE : 브랜드코드
	-->
	<select id="MDMD0020.SEARCH00" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[ 
		SELECT  	/* MDMD0020.SEARCH00 인터넷 개별청약 조회 */ 
					'0' CHEK_FILD		/*체크박스*/
					,A.YEAR_MNTH		/*년월*/
				    ,A.SEQX_FILD		/*일련번호*/
				    ,A.CUST_CODE		/*광고주코드*/
				    ,B.CUST_NAME		/*광고주명*/
				    ,A.CTIM_CODE		/*광고주팀코드*/
				    ,H.CUST_NAME CTIM_NAME	/*광고주팀명*/
				    ,A.BRND_CODE		/*브랜드코드*/
				    ,C.BRND_NAME		/*브랜드명*/
				    ,A.MEDX_CODE	/*청구지코드*/
				    ,D.CUST_NAME MEDX_NAME	/*청구지명*/
				    ,TO_CHAR(SYSDATE,'YYYYMMDD') TRAN_DATE    /*청구일*/
				    ,A.RMED_CODE		/*매체코드*/
				    ,E.CUST_NAME RMED_NAME	/*매체명*/
				    ,A.APLY_DATE		/*적용일자*/
				    ,A.MATT_NAME	/*소재명*/
				    ,A.EMPL_CODE		/*사원코드*/
				    ,SF_GET_EMPLNAME(A.EMPL_CODE) AS EMPL_NAME	/*사원명*/
				    ,A.DEPT_CODE		/*부서코드*/
				    ,G.DEPT_NAME		/*부서명*/
				    ,A.STAR_DATE		/*시작일*/
					,A.ENDD_DATE		/*종료일*/
				    ,A.AMNT_FILD		/*광고비*/
				    ,A.COMI_FILD		/*수수료*/
				    ,A.COMI_RATE		/*수수료율*/
				    ,A.TRTX_FLAG		/*위수탁부가세유무*/
				    ,A.CMTX_FLAG		/*수수료부가세유무*/
				    ,A.TRTR_NUMB		/*위수탁거래명세서번호*/
				    ,A.TRTX_NUMB		/*위수탁세금계산서번호*/
				    ,A.TRCH_NUMB		/*위수탁임시전표번호*/
				    ,A.CMTR_NUMB	/*수수료거래명세서번호*/
				    ,A.CMTX_NUMB	/*수수료세금계산서번호*/
				   	,A.CMCH_NUMB	/*수수료임시전표번호*/
				    ,A.MEMO_FILD		/*비고*/
				    ,A.AORX_FLAG		/*AOR구분*/
				    ,A.INST_USID		/*입력자*/
				    ,A.INST_DATE		/*입력일*/
				    ,A.UPDT_USID		/*수정자*/
				    ,A.UPDT_DATE		/*수정일*/
				FROM MD_ITMDXM A
				LEFT JOIN TA_CUSTXM B ON A.CUST_CODE = B.CUST_CODE
				LEFT JOIN MD_BRNDXM C ON A.BRND_CODE = C.BRND_CODE AND A.CUST_CODE = C.CUST_CODE
				LEFT JOIN TA_CUSTXM D ON A.MEDX_CODE = D.CUST_CODE
				LEFT JOIN MD_CUSTXM E ON A.RMED_CODE = E.CUST_CODE
				LEFT JOIN TM_DEPTXM G ON A.DEPT_CODE = G.DEPT_CODE
				LEFT JOIN MD_CUSTXM H ON A.CTIM_CODE = H.CUST_CODE
	]]>
	<dynamic prepend="WHERE">
		<isNotEmpty prepend="AND">
				A.TRTR_NUMB IS NULL		/* 거래명세유무 */
		</isNotEmpty>
		<isNotEmpty prepend="AND" property="YEAR_MNTH">
				A.YEAR_MNTH LIKE '%' || #YEAR_MNTH# || '%'		/* 년월 */
		</isNotEmpty>
	    <isNotEmpty prepend="AND" property="CUST_CODE">
				A.CUST_CODE LIKE '%' || #CUST_CODE# || '%'		/* 광고주코드 */
		</isNotEmpty>		
		<isNotEmpty prepend="AND" property="BRND_CODE">
				A.BRND_CODE LIKE '%' || #BRND_CODE# || '%'		/* 브랜드코드 */
		</isNotEmpty>	
	</dynamic>
	<![CDATA[
		 ORDER BY TO_NUMBER(A.SEQX_FILD)
	]]>
	</select>
	
	<!-- 
		작  성  자 :  최제현
		작  성  일 : 2016.10.19(표준화 완료)
		내      용  : 인터넷 거래명세서(헤더) 조회
		파라미터  : YEAR_MNTH : 년월, CUST_CODE : 광고주코드, BRND_CODE : 브랜드코드
	-->
	<select id="MDMD0020.SEARCH01" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[ 
		SELECT  	/* MDMD0020.SEARCH01 인터넷 거래명세서(헤더) 조회 */ 
				 '0' CHEK_FILD		/*체크박스*/
                 ,  A.YEAR_MNTH              /* 년월 */
                 , A.TRTR_NUMB              /* 거래명세서번호 */
                 , A.CUST_CODE       	       /* 광고주코드 */
                 , B.CUST_NAME	     		   	/*광고주명*/
                 , A.MEDX_CODE         		     /* 매체사코드 */
                 , E.CUST_NAME RMED_NAME	/*매체명*/
                 , I.COMM_CDNM         		     /* 매체종류 */
                 , D.CUST_NAME MEDX_NAME	/*청구지명*/
                 , A.DEPT_CODE       		       /* 담당부서 */
                 , A.TRAN_DATE              		/* 청구일자 */
                 , A.AMNT_FILD            		  /* 광고대금 */
                 , A.VATX_FILD             		 /* 부가세 */
                 , A.SUMX_FILD             		 /* 합계금액 */
                 , A.MEMO_FILD             		 /* 비고 */
                 , A.TREC_DATE              		/*  */
                 , A.AREC_DATE              		/*  */
                 , A.CREC_DATE              		/*  */
                 , A.FLAG_DATE              		/* 구분날짜 */
              FROM MD_TRANXH A             
				LEFT JOIN TA_CUSTXM B ON A.CUST_CODE = B.CUST_CODE
				LEFT JOIN TA_CUSTXM D ON A.MEDX_CODE = D.CUST_CODE
				LEFT JOIN MD_CUSTXM E ON A.RMED_CODE = E.CUST_CODE
				LEFT JOIN MD_CODEXM I ON A.MEDX_FLAG = I.COMM_CODE
	]]>
	<dynamic prepend="WHERE">
		<isNotEmpty prepend="AND">
				A.MEDX_FLAG = 'A006'	/* 매체구분 */
		</isNotEmpty>
		<isNotEmpty prepend="AND" property="YEAR_MNTH">
				A.YEAR_MNTH LIKE '%' || #YEAR_MNTH# || '%'		/* 년월 */
		</isNotEmpty>
	    <isNotEmpty prepend="AND" property="CUST_CODE">
				A.CUST_CODE LIKE '%' || #CUST_CODE# || '%'		/* 광고주코드 */
		</isNotEmpty>		
		<isNotEmpty prepend="AND" property="BRND_CODE">
				A.BRND_CODE LIKE '%' || #BRND_CODE# || '%'		/* 브랜드코드 */
		</isNotEmpty>	
	</dynamic>
	<![CDATA[
		 ORDER BY TO_NUMBER(A.TRTR_NUMB)
	]]>
	</select>
	
	
	<!-- 
		작  성  자 :  최제현
		작  성  일 : 2016.10.19(표준화 완료)
		내      용  : 인터넷 거래명세서(상세) 조회
		파라미터  : YEAR_MNTH : 년월, CUST_CODE : 광고주코드, BRND_CODE : 브랜드코드
	-->
	<select id="MDMD0020.SEARCH02" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[ 
		SELECT  	/* MDMD0020.SEARCH02 인터넷 거래명세서(상세) 조회 */ 
                 '0' CHEK_FILD		/*체크박스*/
                 , A.YEAR_MNTH              /* 년월 */
                 , A.TRTR_NUMB              /* 거래명세서번호 */
                 , A.SEQX_FILD              /* 개별청약번호 */
                 , A.CUST_CODE              /* 광고주코드 */
                 , B.CUST_NAME	     		   	/*광고주명*/
                 , A.CTIM_CODE              /* 광고주팀코드 */
                 , H.CUST_NAME CTIM_NAME	/*광고주팀명*/
                 , A.MEDX_CODE              /* 청구지코드 */
                 , D.CUST_NAME MEDX_NAME	/*청구지명*/
                 , A.MEDX_FLAG              /* 매체지코드 */
                 , A.MATT_NAME              /* 프로그램 */
                 , A.ADVR_DATE              /* 광고일 */
                 , A.CONT_FILD              /* 회수 */
                 , A.PRCE_FILD              /* 단가 */
                 , A.AMNT_FILD              /* 금액 */
                 , A.VATX_FILD              /* 부가세 */
                 , A.SUMX_FILD              /* 합계금액 */
                 , A.AREA_FLAG              /* 지역코드 */
                 , A.MEMO_FILD              /* 비고 */
                 , A.VATX_FLAG              /* 부가세 */
                 , A.TSEQ_FILD              /* 세부거래명세서번호 */
                 , A.BRND_CODE              /* 브랜드코드 */
                 , C.BRND_NAME	     		   	/*브랜드명*/
                 , A.RMED_CODE              /* 매체코드 */
                 , E.CUST_NAME RMED_NAME	/*매체명*/
                 , A.FLAG_DATE              /* 구분날짜 */
              FROM MD_TRANXD A              
				LEFT JOIN TA_CUSTXM B ON A.CUST_CODE = B.CUST_CODE
				LEFT JOIN MD_BRNDXM C ON A.BRND_CODE = C.BRND_CODE AND A.CUST_CODE = C.CUST_CODE
				LEFT JOIN TA_CUSTXM D ON A.MEDX_CODE = D.CUST_CODE
				LEFT JOIN MD_CUSTXM E ON A.RMED_CODE = E.CUST_CODE
				LEFT JOIN MD_CUSTXM H ON A.CTIM_CODE = H.CUST_CODE
				WHERE 
				A.MEDX_FLAG = 'A006'	/* 매체구분 */
				AND A.YEAR_MNTH LIKE '%' || #YEAR_MNTH# || '%'		/* 년월 */
		 		AND TRTR_NUMB= #TRTR_NUMB#   						       /*거래명세서(헤더) 번호*/
		 		ORDER BY TO_NUMBER(A.TSEQ_FILD)  												/*거래명세서(세부)번호*/
	]]>
	</select>
	
	<!-- 
		작  성  자 :  최제현
		작  성  일 : 2016.11.15(표준화 완료)
		내      용  : 광고주목록 조회
		파라미터  : 
	-->
	<select id="MDMD0020.SEARCH03" parameterClass="hashmap" resultClass="hashmap">
	<![CDATA[ 
		SELECT                                         /* MDMA0030.SEARCH01 광고주목록 가져오기 */
                A.CUST_CODE								/* 광고주코드 */
                , A.CUST_NAME                                 /* 거래처명 */
                , A.CUST_NAME AS CLIENT_NAME    /* 거래처명 */
                , ' ' AS USEX_CODE                          /* NULL */
                FROM TA_CUSTXM A 
                LEFT JOIN TA_CUSTXD B ON A.CUST_CODE = B.CUST_CODE
                WHERE 1=1
                AND B.CUCL_CODE = 'FA260110'
	]]>
	</select>

	<!-- 
		작  성  자 : 최제현
		작  성  일 : 2016.10.18(표준화 완료)
		내      용 : 인터넷 거래명세서 헤더 저장 처리
	-->
	<insert id="MDMD0020.SAVE00" parameterClass="hashmap">
	<![CDATA[
		INSERT INTO MD_TRANXH (  /* MDMD0020.SAVE00 인터넷 거래명세서 헤더 입력 */
                YEAR_MNTH        /* 년월 */
               ,TRTR_NUMB        /* 거래명세서번호 */
               ,CUST_CODE        /* 광고주코드 */
               ,MEDX_CODE        /* 매체사코드 */
               ,MEDX_FLAG        /* 매체종류 */
               ,DEPT_CODE        /* 담당부서 */
               ,TRAN_DATE        /* 청구일자 */
               ,AMNT_FILD        /* 광고대금 */
               ,VATX_FILD        /* 부가세 */
               ,SUMX_FILD        /* 합계금액 */
               ,MEMO_FILD        /* 비고 */
               ,INST_USID        /* 입력자 */
               ,INST_DATE        /* 입력일 */
               ,UPDT_USID        /* 수정자 */
               ,UPDT_DATE        /* 수정일 */
               ,TREC_DATE        /*  */
               ,AREC_DATE        /*  */
               ,CREC_DATE        /*  */
               ,RMED_CODE     /* 매체 코드 */
               ,FLAG_DATE        /* 구분날짜 */
              ) VALUES (
               #YEAR_MNTH#       /* 년월 */
              ,#TRTR_NUMB#       /* 거래명세서번호 */
              ,#CUST_CODE#       /* 광고주코드 */
              ,#MEDX_CODE#       /* 매체사코드 */
              ,'A006'       /* 매체종류 */
              ,#DEPT_CODE#       /* 담당부서 */
              ,#TRAN_DATE#       /* 청구일자 */
              ,#AMNT_FILD#       /* 광고대금 */
              ,#AMNT_FILD#*0.1   /* 부가세 */
              ,#AMNT_FILD#*1.1   /* 합계금액 */
              ,#MEMO_FILD#       /* 비고 */
              ,#UPDT_USID#       /* 입력자 */
              ,#UPDT_DATE#       /* 입력일 */
              ,#UPDT_USID#       /* 수정자 */
              ,#UPDT_DATE#       /* 수정일 */
              ,#TREC_DATE#       /*  */
              ,#AREC_DATE#       /*  */
              ,#CREC_DATE#       /*  */
              ,#RMED_CODE#     /* 매체 코드 */
              ,#FLAG_DATE#       /* 구분날짜 */
              )
	]]>
	</insert>
	
<!-- 
		작  성  자 : 최제현
		작  성  일 : 2016.10.19(표준화 완료)
		내      용 : 인터넷 거래명세서 세부 저장 처리
	-->
	<insert id="MDMD0020.SAVE01" parameterClass="hashmap">
	<![CDATA[
       INSERT INTO MD_TRANXD (  /* MDMD0020.SAVE01 인터넷 거래명세서(상세) 입력 */
                YEAR_MNTH        /* 년월 */
               ,TRTR_NUMB        /* 거래명세서번호 */
               ,SEQX_FILD        /* 개별청약번호 */
               ,CUST_CODE        /* 광고주코드 */
               ,CTIM_CODE        /* 광고주팀코드 */
               ,MEDX_CODE        /* 매체사코드 */
               ,MEDX_FLAG        /* 매체지코드 */
               ,MATT_NAME        /* 프로그램 */
               ,ADVR_DATE        /* 광고일 */
               ,CONT_FILD        /* 회수 */
               ,PRCE_FILD        /* 단가 */
               ,AMNT_FILD        /* 금액 */
               ,VATX_FILD        /* 부가세 */
               ,SUMX_FILD        /* 합계금액 */
               ,AREA_FLAG        /* 지역코드 */
               ,INST_USID        /* 작성자 */
               ,INST_DATE        /* 작성일 */
               ,UPDT_USID        /* 수정자 */
               ,UPDT_DATE        /* 수정일 */
               ,MEMO_FILD        /* 비고 */
               ,VATX_FLAG        /* 부가세 */
               ,TSEQ_FILD        /* 세부거래명세서번호 */
               ,BRND_CODE        /* 브랜드코드 */
               ,RMED_CODE        /* 매체코드 */
               ,FLAG_DATE        /* 구분날짜 */
               ,DEPT_CODE			/* 부서코드 */
              )
       VALUES (
               #YEAR_MNTH#       /* 년월 */
              ,#TRTR_NUMB#       /* 거래명세서번호 */
              ,#SEQX_FILD#       /* 개별청약번호 */
              ,#CUST_CODE#       /* 광고주코드 */
              ,#CTIM_CODE#       /* 광고주팀코드 */
              ,#MEDX_CODE#       /* 매체사코드 */
              ,'A006'       /* 매체종류 */
              ,#MATT_NAME#       /* 프로그램 */
              ,#STAR_DATE#||'~'||#ENDD_DATE#       /* 광고일 */
              ,''       /* 회수 */
              ,#AMNT_FILD#       /* 단가 */
              ,#AMNT_FILD#       /* 금액 */
              ,#AMNT_FILD#*0.1   /* 부가세 */
              ,#AMNT_FILD#*1.1   /* 합계금액 */
              ,''       /* 지역코드 */
              ,#UPDT_USID#       /* 작성자 */
              ,#UPDT_DATE#       /* 작성일 */
              ,#UPDT_USID#       /* 수정자 */
              ,#UPDT_DATE#       /* 수정일 */
              ,#MEMO_FILD#       /* 비고 */
              ,#VATX_FLAG#       /* 부가세 */
              ,#TSEQ_FILD#      /* 세부거래명세서번호 */
              ,#BRND_CODE#       /* 브랜드코드 */
              ,#RMED_CODE#       /* 매체코드 */
              ,#FLAG_DATE#       /* FLAG_DATE */
              ,#DEPT_CODE#		/* 부서코드 */
              )
	]]>
	</insert>
	

	<!-- 
		작  성  자 : 최제현
		작  성  일 : 2016.10.19(표준화 완료)
		내      용 : 인터넷 개별청약 거래명세번호 입력 처리
	-->
	<update id="MDMD0020.UPDATE00" parameterClass="hashmap">
	<![CDATA[
		UPDATE MD_ITMDXM    /* MDMD0020.UPDATE00 인터넷 개별청약 거래명세번호 입력 */
           SET 
             TRTR_NUMB = #TRTR_NUMB#       /* 거래명세번호 */
         WHERE YEAR_MNTH=#YEAR_MNTH# 				/*년월*/
         AND SEQX_FILD 	= #SEQX_FILD#			/*일련번호*/ 
	]]>
	</update>
	
	<!-- 
		작  성  자 : 최제현
		작  성  일 : 2016.10.19(표준화 완료)
		내      용 : 인터넷 개별청약 거래명세번호 삭제 처리
	-->
	<update id="MDMD0020.UPDATE01" parameterClass="hashmap">
	<![CDATA[
		UPDATE MD_ITMDXM    /* MDMD0020.UPDATE01 인터넷 개별청약 거래명세번호 삭제 */
           SET 
             TRTR_NUMB = ''  /* 거래명세번호 */
         WHERE YEAR_MNTH=#YEAR_MNTH# 				/*년월*/
         AND TRTR_NUMB = #TRTR_NUMB#			/*거래번호*/ 
	]]>
	</update>
	
	<!-- 
		작  성  자 : 최제현
		작  성  일 : 2016.10.19(표준화 완료)
		내      용 : 광고주별 거래명세서(헤더) 업데이트(금액 합산)
	-->
	<update id="MDMD0020.UPDATE02" parameterClass="hashmap">
	<![CDATA[
		UPDATE MD_TRANXH    /* MDMD0020.UPDATE02  광고주별 거래명세서(헤더) 업데이트(금액 합산) */
           SET 
             AMNT_FILD = AMNT_FILD + #AMNT_FILD#  /* 광고대금 */
             , VATX_FILD = VATX_FILD + #AMNT_FILD#*0.1  /* 부가세 */
             , SUMX_FILD = SUMX_FILD + #AMNT_FILD#*1.1  /* 합계금액 */
             WHERE MEDX_FLAG = 'A006'	/* 매체구분 */
             AND YEAR_MNTH=#YEAR_MNTH#			 /*년월*/
             AND TRTR_NUMB=#TRTR_NUMB#			/*거래명세서번호*/
	]]>
	</update>
	
	<!-- 
		작  성  자 : 최제현
		작  성  일 : 2016.10.19(표준화 완료)
