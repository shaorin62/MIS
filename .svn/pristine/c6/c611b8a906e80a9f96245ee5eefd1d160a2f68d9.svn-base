	<select id="HATF0070P02.SEARCH00" parameterClass="hashmap" resultClass="hashmap">
		<![CDATA[ 
		select  stay_apay, stay_bpay
		from (  select  *
		        from    at_roomct
				where   #BSRP_DATE1# between stdt_date and endd_date
				and     bsrp_code = #BSRP_CODE#
				and     trgt_code = #TRGT_CODE#
		        order by bsrp_code, trgt_code desc, updt_date desc )
		where   rownum = 1
	]]>
	</select>	
	
</sqlMap>
