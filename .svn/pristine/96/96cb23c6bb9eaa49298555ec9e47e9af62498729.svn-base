     작  성  일 : 2016.11.03
     내      용 : 제작의뢰 수정
     파라미터   : 				
     -->
	<update id="PACA0020.UPDATE00" parameterClass="hashmap">
	<![CDATA[
                        UPDATE PD_SATXXH
						SET CONVERSATION_ID = #CONVERSATION_ID# 
						WHERE 1=1
						AND TAXY_MONT = #TAXY_MONT#
						AND TAXX_NUMB = #TAXX_NUMB# 

	]]>
	</update>			
	
	<!-- 
     작  성  자 : 오세훈
     작  성  일 : 2016.12.19
     내      용 :  전자세금계산서 데이터 취소
     파라미터   : 				
     -->
	<update id="PACA0020.UPDATE01" parameterClass="hashmap">
	<![CDATA[
                        UPDATE PD_SATXXH
						SET CONVERSATION_ID = NULL
						WHERE 1=1
						AND CONVERSATION_ID = #CONVERSATION_ID# 
          
	]]>
	</update>			
	

<!-- 
/***********************************************************************************
     작  성  자 : 오세훈
     작  성  일 : 2016.11.03
     내      용 : 전자세금계산서 발송 ITEM 저장
     파라미터   : 
/***********************************************************************************     			 				
     -->
	<insert id="PACA0020.SAVE01" parameterClass="hashmap">
	<![CDATA[
        INSERT INTO XXSB_DTI_ITEM (  /* PACA0020.SAVE01.제작 전자세금계산서 발송ITEM [제작전자세금계산서ITEM] */
                CONVERSATION_ID        	/* CONVERSATION_ID  */
               ,SUPBUY_TYPE        			/* 매입/매출 구분 */
               ,DIRECTION        				/* 정/역 구분 */
               ,DTI_LINE_NUM        			/* dti_line_num */
               ,ITEM_CODE        				/* 품목코드 */
               ,ITEM_NAME        				/* 품목명 */
               ,ITEM_SIZE        				/* 규격 */
               ,ITEM_MD        				/* 구입일자 */
               ,UNIT_PRICE        				/* 단가 */
               ,ITEM_QTY        				/* 수량 */
               ,SUP_AMOUNT        			/* 공급가액 */
               ,TAX_AMOUNT        			/* 세액 */
               ,FOREIGN_AMOUNT        	/* 외화금액 */
               ,CURRENCY_CODE        		/* 통화코드 */
               ,ITEM_GUBUN        			/* ITEM 구분값 */
               ,REMARK        					/* 비고 */
               ,CREATED_BY        			/* 작성자 */
               ,CREATION_DATE        		/* 작성일시 */
               ,LAST_UPDATED_BY        	/* 최종수정자 */
               ,LAST_UPDATE_DATE        	/* 최종수정일시 */
              )
      SELECT
                #CONVERSATION_ID#
                ,#SUPBUY_TYPE# 
                ,#DIRECTION# 
                ,A.TAXX_IDXX  
                ,#ITEM_CODE# 
                , CASE WHEN NVL(A.ITEM_MEMO,' ') = ' ' THEN B.JOBN_NAME ELSE A.ITEM_MEMO END
                ,B.JOBN_SIZE 
                ,A.DEND_DATE 
                ,#UNIT_PRICE# 
                ,#ITEM_QTY#          
                ,NVL(A.TAXX_AMTX,0) 
                ,NVL(A.TAXX_VATX,0)        
                ,#FOREIGN_AMOUNT#    
                ,#CURRENCY_CODE# 
                ,'DTI' 
                ,B.MEMO_FILD   
                ,#UPDT_USID# 
                ,SYSDATE     
                ,#UPDT_USID#   
                ,SYSDATE             
                FROM PD_SATXXD A 
                LEFT JOIN PD_JOBNXM B ON A.JOBN_NUMB = B.JOBN_NUMB
                WHERE 1=1 
                AND A.TAXY_MONT = #TAXY_MONT#
                AND A.TAXX_NUMB = #TAXX_NUMB#

	]]>
	</insert>	
	
	

