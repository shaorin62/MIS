			               ,ETCNUM3        							/* 기타번호3 */
			               ,ETCNUM4       								/* 기타번호4 */
			               ,DTI_MSG        							/* 세금계산서 원본 */
			               ,AMEND_CODE        						/* 수정코드 */
			               ,REMARK2        							/* 비고2 */
			               ,REMARK3        							/* 비고3 */
